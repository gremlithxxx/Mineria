      "source": [
        "# ============================================================\n",
        "# FIGURA 1: Units Produced vs Defect Rate (Dispersión)\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"GENERANDO FIGURA 1\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Crear la figura\n",
        "fig, ax = plt.subplots(figsize=(10, 6))\n",
        "\n",
        "# Graficar puntos por línea de producción\n",
        "for line in df[production_line_col].unique():\n",
        "    line_data = df[df[production_line_col] == line]\n",
        "\n",
        "    ax.scatter(\n",
        "        line_data[units_col],\n",
        "        line_data[defect_rate_col] / 100,  # Convertir a proporción para formato %\n",
        "        label=line,\n",
        "        color=colors_lines.get(line, '#333333'),\n",
        "        marker=markers_lines.get(line, 'o'),\n",
        "        alpha=0.7,\n",
        "        edgecolors='black',\n",
        "        linewidth=0.5,\n",
        "        s=50\n",
        "    )\n",
        "\n",
        "# Configurar ejes\n",
        "ax.set_xlabel('Units Produced', fontsize=11)\n",
        "ax.set_ylabel('Defect Rate', fontsize=11)\n",
        "\n",
        "# Formatear eje Y como porcentaje\n",
        "ax.yaxis.set_major_formatter(plt.FuncFormatter(lambda y, _: '{:.0%}'.format(y)))\n",
        "\n",
        "# Configurar cuadrícula\n",
        "ax.grid(True, linestyle='--', linewidth=0.7, alpha=0.6)\n",
        "\n",
        "# Configurar bordes\n",
        "for spine in ax.spines.values():\n",
        "    spine.set_color('#222')\n",
        "    spine.set_linewidth(1.2)\n",
        "\n",
        "# Título\n",
        "ax.set_title('Fig. 1: Units Produced vs Defect Rate', fontsize=13, fontweight='bold', pad=15)\n",
        "\n",
        "# Leyenda\n",
        "ax.legend(title='Production Line', loc='best', frameon=True)\n",
        "\n",
        "# Ajustar ticks\n",
        "plt.xticks(fontsize=10)\n",
        "plt.yticks(fontsize=10)\n",
        "\n",
        "# Ajustar layout\n",
        "plt.tight_layout()\n",
        "\n",
        "# Mostrar figura\n",
        "plt.show()\n",
        "\n",
        "# ============================================================\n",
        "# INTERPRETACIÓN DE LA FIGURA 1\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"INTERPRETACIÓN FIGURA 1\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "print(\"\\nAnálisis de Units Produced vs Defect Rate:\")\n",
        "\n",
        "# Calcular correlación\n",
        "correlation = df[units_col].corr(df[defect_rate_col])\n",
        "print(f\"\\nCorrelación: {correlation:.3f}\")\n",
        "\n",
        "# Determinar tendencia\n",
        "if correlation > 0.1:\n",
        "    tendencia = \"positiva (a más producción, más defectos)\"\n",
        "elif correlation < -0.1:\n",
        "    tendencia = \"negativa (a más producción, menos defectos)\"\n",
        "else:\n",
        "    tendencia = \"sin tendencia clara\"\n",
        "\n",
        "print(f\"Tendencia: {tendencia}\")\n",
        "\n",
        "# Estadísticas por línea\n",
        "print(\"\\nEstadísticas por línea de producción:\")\n",
        "for line in df[production_line_col].unique():\n",
        "    line_data = df[df[production_line_col] == line]\n",
        "    print(f\"\\n{line}:\")\n",
        "    print(f\"  Producción promedio: {line_data[units_col].mean():.0f}\")\n",
        "    print(f\"  Tasa defectos promedio: {line_data[defect_rate_col].mean():.2f}%\")\n",
        "\n",
        "# Identificar outliers potenciales\n",
        "mean_defect = df[defect_rate_col].mean()\n",
        "std_defect = df[defect_rate_col].std()\n",
        "outliers = df[abs(df[defect_rate_col] - mean_defect) > 2 * std_defect]\n",
        "\n",
        "if len(outliers) > 0:\n",
        "    print(f\"\\nPosibles outliers ({len(outliers)} registros):\")\n",
        "    for idx, row in outliers.iterrows():\n",
        "        print(f\"  {row[production_line_col]}, {row[shift_col]}: {row[units_col]} unidades, {row[defect_rate_col]:.2f}% defectos\")\n",
        "else:\n",
        "    print(\"\\nNo se detectaron outliers significativos\")\n",
        "\n",
        "# Resumen\n",
        "print(\"\\nResumen:\")\n",
        "if correlation > 0.3:\n",
        "    print(\"- Hay una asociación positiva moderada-fuerte entre producción y defectos\")\n",
        "    print(\"- Líneas con mayor producción tienden a tener más defectos\")\n",
        "elif correlation > 0.1:\n",
        "    print(\"- Hay una ligera tendencia positiva, pero la relación no es fuerte\")\n",
        "    print(\"- Otros factores podrían influir en la tasa de defectos\")\n",
        "else:\n",
        "    print(\"- No hay evidencia clara de que aumentar producción incremente defectos\")\n",
        "    print(\"- La calidad se mantiene estable independientemente del volumen\")\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)"
      ]
    },
