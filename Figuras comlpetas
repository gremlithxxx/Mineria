      "cell_type": "code",
      "source": [
        "# ============================================================\n",
        "# FIGURA 7: Cost_per_Unit por Shift (Boxplot)\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"GENERANDO FIGURA 7\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Crear la figura\n",
        "fig, ax = plt.subplots(figsize=(10, 6))\n",
        "\n",
        "# Orden de turnos\n",
        "orden_turnos = ['Morning', 'Evening', 'Night']\n",
        "\n",
        "# Crear listas de datos por turno\n",
        "data_turnos = [df[df[shift_col] == turno][cost_col].dropna() for turno in orden_turnos]\n",
        "\n",
        "# Colores para los boxplots (usando colores de turno con transparencia)\n",
        "box_colors = [colors_shift[turno] for turno in orden_turnos]\n",
        "\n",
        "# Crear boxplot\n",
        "bp = ax.boxplot(\n",
        "    data_turnos,\n",
        "    labels=orden_turnos,\n",
        "    patch_artist=True,\n",
        "    notch=True,\n",
        "    showmeans=True,\n",
        "    meanline=True,\n",
        "    meanprops={'color': 'blue', 'linestyle': '--', 'linewidth': 1.5},\n",
        "    medianprops={'color': 'black', 'linewidth': 2},\n",
        "    whiskerprops={'color': '#222', 'linewidth': 1},\n",
        "    capprops={'color': '#222', 'linewidth': 1},\n",
        "    flierprops={'marker': 'o', 'markerfacecolor': 'gray', 'markersize': 4, 'alpha': 0.5}\n",
        ")\n",
        "\n",
        "# Colorear las cajas\n",
        "for patch, color in zip(bp['boxes'], box_colors):\n",
        "    patch.set_facecolor(color)\n",
        "    patch.set_alpha(0.7)\n",
        "    patch.set_edgecolor('black')\n",
        "    patch.set_linewidth(1)\n",
        "\n",
        "# Configurar ejes\n",
        "ax.set_xlabel('Shift', fontsize=11)\n",
        "ax.set_ylabel('Cost per Unit (USD)', fontsize=11)\n",
        "\n",
        "# Configurar ticks\n",
        "ax.yaxis.set_major_formatter(plt.FuncFormatter(lambda y, _: f'${y:.2f}'))\n",
        "\n",
        "# Configurar cuadrícula (solo eje Y)\n",
        "ax.grid(True, linestyle='--', linewidth=0.7, alpha=0.6, axis='y')\n",
        "\n",
        "# Configurar bordes\n",
        "for spine in ax.spines.values():\n",
        "    spine.set_color('#222')\n",
        "    spine.set_linewidth(1.2)\n",
        "\n",
        "# Título\n",
        "ax.set_title('Fig. 7: Cost_per_Unit por Shift (Boxplot)', fontsize=13, fontweight='bold', pad=15)\n",
        "\n",
        "# Añadir leyenda explicativa\n",
        "from matplotlib.lines import Line2D\n",
        "legend_elements = [\n",
        "    Line2D([0], [0], color='blue', linestyle='--', label='Media'),\n",
        "    Line2D([0], [0], color='black', linewidth=2, label='Mediana'),\n",
        "    Line2D([0], [0], marker='o', color='w', markerfacecolor='gray', markersize=4, label='Outliers')\n",
        "]\n",
        "ax.legend(handles=legend_elements, loc='upper right', frameon=True)\n",
        "\n",
        "# Ajustar layout\n",
        "plt.tight_layout()\n",
        "\n",
        "# Mostrar figura\n",
        "plt.show()\n",
        "\n",
        "# ============================================================\n",
        "# INTERPRETACIÓN DE LA FIGURA 7\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"INTERPRETACIÓN FIGURA 7\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Estadísticas por turno\n",
        "print(\"\\nESTADÍSTICAS DE COSTO POR UNIDAD POR TURNO:\")\n",
        "print(\"-\" * 60)\n",
        "\n",
        "for turno in orden_turnos:\n",
        "    data = df[df[shift_col] == turno][cost_col].dropna()\n",
        "\n",
        "    print(f\"\\n{turno}:\")\n",
        "    print(f\"  Media: ${data.mean():.2f}\")\n",
        "    print(f\"  Mediana: ${data.median():.2f}\")\n",
        "    print(f\"  Desviación estándar: ${data.std():.2f}\")\n",
        "    print(f\"  Mínimo: ${data.min():.2f}\")\n",
        "    print(f\"  Máximo: ${data.max():.2f}\")\n",
        "    print(f\"  Rango intercuartil (IQR): ${data.quantile(0.75) - data.quantile(0.25):.2f}\")\n",
        "    print(f\"  Q1 (25%): ${data.quantile(0.25):.2f}\")\n",
        "    print(f\"  Q3 (75%): ${data.quantile(0.75):.2f}\")\n",
        "    # Corrected line:\n",
        "    print(f\"  Número de outliers: {len(data[(data < (data.quantile(0.25) - 1.5 * (data.quantile(0.75) - data.quantile(0.25)))) | (data > (data.quantile(0.75) + 1.5 * (data.quantile(0.75) - data.quantile(0.25))))])}\")\n",
        "\n",
        "# ============================================================\n",
        "# ANÁLISIS COMPARATIVO ENTRE TURNOS\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"ANÁLISIS COMPARATIVO ENTRE TURNOS\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Comparar medias\n",
        "print(\"\\nCOMPARACIÓN DE MEDIAS:\")\n",
        "medias = {turno: df[df[shift_col] == turno][cost_col].mean() for turno in orden_turnos}\n",
        "turno_menor_costo = min(medias, key=medias.get)\n",
        "turno_mayor_costo = max(medias, key=medias.get)\n",
        "diferencia_medias = medias[turno_mayor_costo] - medias[turno_menor_costo]\n",
        "porcentaje_diferencia = (diferencia_medias / medias[turno_menor_costo]) * 100\n",
        "\n",
        "print(f\"Turno con menor costo promedio: {turno_menor_costo} (${medias[turno_menor_costo]:.2f})\")\n",
        "print(f\"Turno con mayor costo promedio: {turno_mayor_costo} (${medias[turno_mayor_costo]:.2f})\")\n",
        "print(f\"Diferencia: ${diferencia_medias:.2f} ({porcentaje_diferencia:.1f}% más caro)\")\n",
        "\n",
        "# Comparar medianas\n",
        "print(\"\\nCOMPARACIÓN DE MEDIANAS:\")\n",
        "medianas = {turno: df[df[shift_col] == turno][cost_col].median() for turno in orden_turnos}\n",
        "turno_menor_mediana = min(medianas, key=medianas.get)\n",
        "turno_mayor_mediana = max(medianas, key=medianas.get)\n",
        "print(f\"Turno con menor mediana: {turno_menor_mediana} (${medianas[turno_menor_mediana]:.2f})\")\n",
        "print(f\"Turno con mayor mediana: {turno_mayor_mediana} (${medianas[turno_mayor_mediana]:.2f})\")\n",
        "\n",
        "# Comparar variabilidad\n",
        "print(\"\\nCOMPARACIÓN DE VARIABILIDAD:\")\n",
        "std_turnos = {turno: df[df[shift_col] == turno][cost_col].std() for turno in orden_turnos}\n",
        "turno_mas_variable = max(std_turnos, key=std_turnos.get)\n",
        "turno_mas_consistente = min(std_turnos, key=std_turnos.get)\n",
        "\n",
        "print(f\"Turno más variable: {turno_mas_variable} (σ=${std_turnos[turno_mas_variable]:.2f})\")\n",
        "print(f\"Turno más consistente: {turno_mas_consistente} (σ=${std_turnos[turno_mas_consistente]:.2f})\")\n",
        "\n",
        "# Coeficiente de variación\n",
        "print(\"\\nCOEFICIENTE DE VARIACIÓN (CV):\")\n",
        "for turno in orden_turnos:\n",
        "    cv = (std_turnos[turno] / medias[turno]) * 100\n",
        "    print(f\"  {turno}: CV = {cv:.1f}%\")\n",
        "\n",
        "# ============================================================\n",
        "# ANÁLISIS DE OUTLIERS\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"ANÁLISIS DE OUTLIERS\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "for turno in orden_turnos:\n",
        "    data = df[df[shift_col] == turno][cost_col].dropna()\n",
        "    Q1 = data.quantile(0.25)\n",
        "    Q3 = data.quantile(0.75)\n",
        "    IQR = Q3 - Q1\n",
        "    lower_bound = Q1 - 1.5 * IQR\n",
        "    upper_bound = Q3 + 1.5 * IQR\n",
        "\n",
        "    outliers = data[(data < lower_bound) | (data > upper_bound)]\n",
        "\n",
        "    if len(outliers) > 0:\n",
        "        print(f\"\\n{turno} - Outliers detectados ({len(outliers)}):\")\n",
        "        for idx, value in outliers.items():\n",
        "            print(f\"  Lote {df.loc[idx, product_id_col] if product_id_col in df.columns else idx}: ${value:.2f}\")\n",
        "\n",
        "            # Intentar encontrar causa del outlier\n",
        "            if value > upper_bound:\n",
        "                print(f\"    → Costo inusualmente alto\")\n",
        "                # Verificar si tiene bajo producción o alto downtime\n",
        "                if units_col in df.columns and idx in df.index:\n",
        "                    if df.loc[idx, units_col] < df[units_col].quantile(0.25):\n",
        "                        print(\"      Posible causa: baja producción\")\n",
        "                if downtime_col in df.columns and idx in df.index:\n",
        "                    if df.loc[idx, downtime_col] > df[downtime_col].quantile(0.75):\n",
        "                        print(\"      Posible causa: alto tiempo muerto\")\n",
        "    else:\n",
        "        print(f\"\\n{turno}: Sin outliers detectados\")\n",
        "\n",
        "# ============================================================\n",
        "# PRUEBAS ESTADÍSTICAS\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"PRUEBAS ESTADÍSTICAS\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# ANOVA para comparar todos los turnos\n",
        "from scipy import stats\n",
        "\n",
        "print(\"\\nANOVA (comparación de todos los turnos):\")\n",
        "f_stat, p_value_anova = stats.f_oneway(\n",
        "    df[df[shift_col] == 'Morning'][cost_col].dropna(),\n",
        "    df[df[shift_col] == 'Evening'][cost_col].dropna(),\n",
        "    df[df[shift_col] == 'Night'][cost_col].dropna()\n",
        ")\n",
        "print(f\"F-statistic: {f_stat:.4f}\")\n",
        "print(f\"p-value: {p_value_anova:.4f}\")\n",
        "\n",
        "if p_value_anova < 0.05:\n",
        "    print(\"→ DIFERENCIAS SIGNIFICATIVAS entre turnos (p<0.05)\")\n",
        "else:\n",
        "    print(\"→ No hay diferencias significativas entre turnos\")\n",
        "\n",
        "# Pruebas t por pares\n",
        "print(\"\\nPRUEBAS T POR PARES:\")\n",
        "for i in range(len(orden_turnos)):\n",
        "    for j in range(i+1, len(orden_turnos)):\n",
        "        turno1 = orden_turnos[i]\n",
        "        turno2 = orden_turnos[j]\n",
        "\n",
        "        data1 = df[df[shift_col] == turno1][cost_col].dropna()\n",
        "        data2 = df[df[shift_col] == turno2][cost_col].dropna()\n",
        "\n",
        "        t_stat, p_value = stats.ttest_ind(data1, data2)\n",
        "\n",
        "        if p_value < 0.05:\n",
        "            print(f\"{turno1} vs {turno2}: DIFERENCIA SIGNIFICATIVA (p={p_value:.4f})\")\n",
        "        else:\n",
        "            print(f\"{turno1} vs {turno2}: Diferencia no significativa (p={p_value:.4f})\")\n",
        "\n",
        "# ============================================================\n",
        "# INTERPRETACIÓN FINAL\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"INTERPRETACIÓN FINAL\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# 1. Turno más económico\n",
        "print(\"\\n1. EFICIENCIA EN COSTOS:\")\n",
        "if turno_menor_costo == 'Morning':\n",
        "    print(\"   El turno MAÑANA es el más eficiente en costos\")\n",
        "    print(\"   Posiblemente por mejor planificación o personal más experimentado\")\n",
        "elif turno_menor_costo == 'Evening':\n",
        "    print(\"   El turno TARDE es el más eficiente en costos\")\n",
        "    print(\"   Podría deberse a mejor utilización de recursos\")\n",
        "else:\n",
        "    print(\"   El turno NOCHE es el más eficiente en costos\")\n",
        "    print(\"   Interesante: costos más bajos en horario nocturno\")\n",
        "\n",
        "# 2. Consistencia\n",
        "print(\"\\n2. CONSISTENCIA OPERATIVA:\")\n",
        "if turno_mas_consistente == turno_menor_costo:\n",
        "    print(\"   El turno más económico también es el más consistente\")\n",
        "    print(\"   Esto indica procesos bien controlados\")\n",
        "else:\n",
        "    print(f\"   {turno_mas_consistente} es el más consistente, pero no el más económico\")\n",
        "    print(\"   Sugiere trade-off entre consistencia y costo\")\n",
        "\n",
        "# 3. Asimetría (media vs mediana)\n",
        "print(\"\\n3. ASIMETRÍA EN LA DISTRIBUCIÓN:\")\n",
        "for turno in orden_turnos:\n",
        "    data = df[df[shift_col] == turno][cost_col].dropna()\n",
        "    media = data.mean()\n",
        "    mediana = data.median()\n",
        "\n",
        "    if media > mediana:\n",
        "        print(f\"   {turno}: Sesgo positivo (media > mediana)\")\n",
        "        print(f\"     Algunos lotes con costos altos jalan el promedio\")\n",
        "    elif media < mediana:\n",
        "        print(f\"   {turno}: Sesgo negativo (media < mediana)\")\n",
        "        print(f\"     Mayoría de lotes tienen costos por encima de lo normal\")\n",
        "    else:\n",
        "        print(f\"   {turno}: Distribución simétrica\")\n",
        "\n",
        "# 4. Outliers\n",
        "print(\"\\n4. OUTLIERS Y CASOS ESPECIALES:\")\n",
        "outliers_por_turno = {}\n",
        "for turno in orden_turnos:\n",
        "    data = df[df[shift_col] == turno][cost_col].dropna()\n",
        "    Q1 = data.quantile(0.25)\n",
        "    Q3 = data.quantile(0.75)\n",
        "    IQR = Q3 - Q1\n",
        "    upper_bound = Q3 + 1.5 * IQR\n",
        "    outliers = data[data > upper_bound]\n",
        "    outliers_por_turno[turno] = len(outliers)\n",
        "\n",
        "turno_mas_outliers = max(outliers_por_turno, key=outliers_por_turno.get)\n",
        "if outliers_por_turno[turno_mas_outliers] > 0:\n",
        "    print(f\"   {turno_mas_outliers} tiene más casos de costos anómalos\")\n",
        "    print(f\"   ({outliers_por_turno[turno_mas_outliers]} outliers)\")\n",
        "    print(\"   Investigar causas específicas en estos lotes\")\n",
        "\n",
        "# 5. Recomendaciones\n",
        "print(\"\\n5. RECOMENDACIONES:\")\n",
        "\n",
        "if p_value_anova < 0.05:\n",
        "    print(f\"   • Implementar prácticas del turno {turno_menor_costo} en los demás\")\n",
        "    print(f\"   • Potencial ahorro: ${diferencia_medias:.2f} por unidad\")\n",
        "\n",
        "    if turno_mayor_costo == 'Night':\n",
        "        print(\"   • Revisar turno noche: costos más altos, posiblemente por:\")\n",
        "        print(\"     - Menor supervisión\")\n",
        "        print(\"     - Fatiga del personal\")\n",
        "        print(\"     - Mantenimiento nocturno\")\n",
        "else:\n",
        "    print(\"   • No hay diferencias significativas entre turnos\")\n",
        "    print(\"   • Mantener prácticas actuales\")\n",
        "    print(\"   • Enfocar en reducción general de costos\")\n",
        "\n",
        "if turno_mas_variable != turno_mas_consistente:\n",
        "    print(f\"   • Reducir variabilidad en {turno_mas_variable}\")\n",
        "    print(\"     Estandarizar procesos y capacitar personal\")\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n"
      ],
