      "cell_type": "code",
      "source": [
        "# ============================================================\n",
        "# FIGURA 9: Densidad de Energy_Consumption_kWh (KDE) - SIN EMOJIS\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"GENERANDO FIGURA 9\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Crear la figura\n",
        "fig, ax = plt.subplots(figsize=(10, 6))\n",
        "\n",
        "# Calcular estadísticas\n",
        "mean_energy = df[energy_col].mean()\n",
        "median_energy = df[energy_col].median()\n",
        "\n",
        "# Crear curva KDE\n",
        "from scipy import stats\n",
        "kde = stats.gaussian_kde(df[energy_col].dropna())\n",
        "x_range = np.linspace(df[energy_col].min(), df[energy_col].max(), 200)\n",
        "\n",
        "# Graficar KDE\n",
        "ax.plot(x_range, kde(x_range), color='#111', linewidth=2.5, label='KDE')\n",
        "\n",
        "# Sombrear área bajo la curva\n",
        "ax.fill_between(x_range, kde(x_range), alpha=0.3, color='#1f77b4')\n",
        "\n",
        "# Línea de media\n",
        "ax.axvline(mean_energy, color='green', linestyle='--', linewidth=2,\n",
        "           label=f'Media: {mean_energy:.1f} kWh')\n",
        "\n",
        "# Línea de mediana\n",
        "ax.axvline(median_energy, color='red', linestyle='-.', linewidth=2,\n",
        "           label=f'Mediana: {median_energy:.1f} kWh')\n",
        "\n",
        "# Configurar ejes\n",
        "ax.set_xlabel('Energy Consumption (kWh)', fontsize=11)\n",
        "ax.set_ylabel('Densidad', fontsize=11)\n",
        "\n",
        "# Configurar cuadrícula\n",
        "ax.grid(True, linestyle='--', linewidth=0.7, alpha=0.6)\n",
        "\n",
        "# Configurar bordes\n",
        "for spine in ax.spines.values():\n",
        "    spine.set_color('#222')\n",
        "    spine.set_linewidth(1.2)\n",
        "\n",
        "# Título\n",
        "ax.set_title('Fig. 9: Densidad de Energy_Consumption_kWh', fontsize=13, fontweight='bold', pad=15)\n",
        "\n",
        "# Leyenda\n",
        "ax.legend(loc='upper right', frameon=True)\n",
        "\n",
        "# Ajustar layout\n",
        "plt.tight_layout()\n",
        "\n",
        "# Mostrar figura\n",
        "plt.show()\n",
        "\n",
        "# ============================================================\n",
        "# INTERPRETACIÓN DE LA FIGURA 9\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"INTERPRETACIÓN FIGURA 9\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# ============================================================\n",
        "# 1. ESTADÍSTICAS DESCRIPTIVAS\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n1. ESTADÍSTICAS DE CONSUMO ENERGÉTICO:\")\n",
        "print(\"-\" * 60)\n",
        "\n",
        "print(f\"Media: {mean_energy:.1f} kWh\")\n",
        "print(f\"Mediana: {median_energy:.1f} kWh\")\n",
        "print(f\"Desviación estándar: {df[energy_col].std():.1f} kWh\")\n",
        "print(f\"Mínimo: {df[energy_col].min():.1f} kWh\")\n",
        "print(f\"Máximo: {df[energy_col].max():.1f} kWh\")\n",
        "print(f\"Rango: {df[energy_col].max() - df[energy_col].min():.1f} kWh\")\n",
        "\n",
        "# Percentiles\n",
        "percentiles = [10, 25, 50, 75, 90]\n",
        "print(f\"\\nPercentiles:\")\n",
        "for p in percentiles:\n",
        "    valor = df[energy_col].quantile(p/100)\n",
        "    print(f\"  P{p}: {valor:.1f} kWh\")\n",
        "\n",
        "# ============================================================\n",
        "# 2. ANÁLISIS DE MODAS (MULTIMODALIDAD)\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"2. ANÁLISIS DE MODAS\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Encontrar picos en KDE\n",
        "from scipy.signal import find_peaks\n",
        "\n",
        "# Suavizar un poco para detectar picos\n",
        "kde_x = x_range\n",
        "kde_y = kde(x_range)\n",
        "peaks, properties = find_peaks(kde_y, height=0.001, distance=20)\n",
        "\n",
        "print(f\"Número de modas detectadas: {len(peaks)}\")\n",
        "\n",
        "if len(peaks) > 1:\n",
        "    print(\"\\nModas encontradas (kWh):\")\n",
        "    for i, peak_idx in enumerate(peaks):\n",
        "        print(f\"  Moda {i+1}: {kde_x[peak_idx]:.1f} kWh (densidad: {kde_y[peak_idx]:.4f})\")\n",
        "\n",
        "    print(f\"\\n→ Distribución MULTIMODAL ({len(peaks)} picos)\")\n",
        "    print(\"  Esto sugiere diferentes regímenes de operación o tipos de producción\")\n",
        "else:\n",
        "    print(\"\\n→ Distribución UNIMODAL (un solo pico)\")\n",
        "    print(f\"  Moda principal: {kde_x[peaks[0]]:.1f} kWh\")\n",
        "\n",
        "# ============================================================\n",
        "# 3. ANÁLISIS DE ASIMETRÍA\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"3. ANÁLISIS DE ASIMETRÍA\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "skewness = df[energy_col].skew()\n",
        "print(f\"Coeficiente de asimetría (skewness): {skewness:.3f}\")\n",
        "\n",
        "if skewness > 0.5:\n",
        "    print(\"→ Sesgo POSITIVO (cola derecha más larga)\")\n",
        "    print(\"  Hay períodos con consumo energético excepcionalmente alto\")\n",
        "elif skewness < -0.5:\n",
        "    print(\"→ Sesgo NEGATIVO (cola izquierda más larga)\")\n",
        "    print(\"  Hay períodos con consumo energético excepcionalmente bajo\")\n",
        "else:\n",
        "    print(\"→ Distribución aproximadamente SIMÉTRICA\")\n",
        "    print(\"  El consumo energético es equilibrado alrededor de la media\")\n",
        "\n",
        "# Relación media-mediana\n",
        "if mean_energy > median_energy:\n",
        "    diferencia = ((mean_energy - median_energy) / median_energy) * 100\n",
        "    print(f\"\\nMedia > Mediana por {diferencia:.1f}%\")\n",
        "    print(\"  Los valores altos jalan la media hacia arriba\")\n",
        "elif mean_energy < median_energy:\n",
        "    diferencia = ((median_energy - mean_energy) / median_energy) * 100\n",
        "    print(f\"\\nMediana > Media por {diferencia:.1f}%\")\n",
        "    print(\"  Los valores bajos jalan la media hacia abajo\")\n",
        "else:\n",
        "    print(\"\\nMedia = Mediana (distribución perfectamente simétrica)\")\n",
        "\n",
        "# ============================================================\n",
        "# 4. ANÁLISIS POR LÍNEA DE PRODUCCIÓN\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"4. CONSUMO ENERGÉTICO POR LÍNEA\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "for linea in df[production_line_col].unique():\n",
        "    datos_linea = df[df[production_line_col] == linea][energy_col].dropna()\n",
        "\n",
        "    print(f\"\\n{linea}:\")\n",
        "    print(f\"  Media: {datos_linea.mean():.1f} kWh\")\n",
        "    print(f\"  Mediana: {datos_linea.median():.1f} kWh\")\n",
        "    print(f\"  Std: {datos_linea.std():.1f} kWh\")\n",
        "\n",
        "    # Comparación con promedio global\n",
        "    if datos_linea.mean() > mean_energy * 1.1:\n",
        "        print(f\"  Consumo {((datos_linea.mean()/mean_energy - 1)*100):.0f}% superior al promedio\")\n",
        "    elif datos_linea.mean() < mean_energy * 0.9:\n",
        "        print(f\"  Consumo {((1 - datos_linea.mean()/mean_energy)*100):.0f}% inferior al promedio\")\n",
        "    else:\n",
        "        print(f\"  Consumo dentro del promedio global\")\n",
        "\n",
        "# ============================================================\n",
        "# 5. ANÁLISIS POR TURNO\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"5. CONSUMO ENERGÉTICO POR TURNO\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "turnos = ['Morning', 'Evening', 'Night']\n",
        "for turno in turnos:\n",
        "    datos_turno = df[df[shift_col] == turno][energy_col].dropna()\n",
        "\n",
        "    if len(datos_turno) > 0:\n",
        "        print(f\"\\n{turno}:\")\n",
        "        print(f\"  Media: {datos_turno.mean():.1f} kWh\")\n",
        "        print(f\"  Mediana: {datos_turno.median():.1f} kWh\")\n",
        "        print(f\"  Std: {datos_turno.std():.1f} kWh\")\n",
        "\n",
        "# ============================================================\n",
        "# 6. RELACIÓN CON PRODUCCIÓN\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"6. EFICIENCIA ENERGÉTICA\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Calcular consumo por unidad producida\n",
        "df['Energy_per_Unit'] = df[energy_col] / df[units_col]\n",
        "eficiencia_media = df['Energy_per_Unit'].mean()\n",
        "\n",
        "print(f\"Consumo energético por unidad producida: {eficiencia_media:.2f} kWh/unidad\")\n",
        "\n",
        "# Comparar por línea\n",
        "print(\"\\nEficiencia por línea (kWh/unidad):\")\n",
        "for linea in df[production_line_col].unique():\n",
        "    datos_linea = df[df[production_line_col] == linea]\n",
        "    eficiencia = (datos_linea[energy_col] / datos_linea[units_col]).mean()\n",
        "    print(f\"  {linea}: {eficiencia:.2f}\")\n",
        "\n",
        "    if eficiencia < eficiencia_media * 0.95:\n",
        "        print(f\"    Más eficiente que el promedio\")\n",
        "    elif eficiencia > eficiencia_media * 1.05:\n",
        "        print(f\"    Menos eficiente que el promedio\")\n",
        "\n",
        "# ============================================================\n",
        "# 7. DETECCIÓN DE OUTLIERS\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"7. OUTLIERS EN CONSUMO ENERGÉTICO\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Método IQR\n",
        "Q1 = df[energy_col].quantile(0.25)\n",
        "Q3 = df[energy_col].quantile(0.75)\n",
        "IQR = Q3 - Q1\n",
        "lower_bound = Q1 - 1.5 * IQR\n",
        "upper_bound = Q3 + 1.5 * IQR\n",
        "\n",
        "outliers = df[(df[energy_col] < lower_bound) | (df[energy_col] > upper_bound)]\n",
        "\n",
        "print(f\"Número de outliers: {len(outliers)} ({len(outliers)/len(df)*100:.1f}% de los datos)\")\n",
        "\n",
        "if len(outliers) > 0:\n",
        "    print(\"\\nOutliers detectados:\")\n",
        "    for idx, row in outliers.iterrows():\n",
        "        print(f\"  Lote {int(row[product_id_col]) if product_id_col in df.columns else idx}:\")\n",
        "        print(f\"    Consumo: {row[energy_col]:.1f} kWh\")\n",
        "        print(f\"    Línea: {row[production_line_col]}, Turno: {row[shift_col]}\")\n",
        "        print(f\"    Producción: {row[units_col]} unidades\")\n",
        "\n",
        "        # Clasificar outlier\n",
        "        if row[energy_col] > upper_bound:\n",
        "            print(f\"    → Consumo ANORMALMENTE ALTO\")\n",
        "            # Posibles causas\n",
        "            if row[units_col] > df[units_col].quantile(0.75):\n",
        "                print(\"      (puede deberse a alta producción)\")\n",
        "            if row[downtime_col] > df[downtime_col].quantile(0.75):\n",
        "                print(\"      (coincide con alto tiempo muerto)\")\n",
        "        elif row[energy_col] < lower_bound:\n",
        "            print(f\"    → Consumo ANORMALMENTE BAJO\")\n",
        "            if row[units_col] < df[units_col].quantile(0.25):\n",
        "                print(\"      (puede deberse a baja producción)\")\n",
        "\n",
        "# ============================================================\n",
        "# 8. INTERPRETACIÓN FINAL\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"8. INTERPRETACIÓN FINAL\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Perfil de consumo\n",
        "print(\"\\nPERFIL DE CONSUMO ENERGÉTICO:\")\n",
        "\n",
        "if len(peaks) > 1:\n",
        "    print(\"• Múltiples modas: diferentes modos de operación\")\n",
        "    print(\"  Sugiere cambios de turno, diferentes productos o regímenes\")\n",
        "else:\n",
        "    print(\"• Operación estable: un solo régimen de consumo predominante\")\n",
        "\n",
        "# Eficiencia\n",
        "print(f\"\\nEFICIENCIA PROMEDIO: {eficiencia_media:.2f} kWh/unidad\")\n",
        "\n",
        "# Comparación con benchmark teórico (asumiendo 0.5 kWh/unidad como referencia)\n",
        "benchmark = 0.5\n",
        "if eficiencia_media < benchmark:\n",
        "    print(f\"✓ Eficiencia superior al benchmark ({benchmark} kWh/unidad)\")\n",
        "else:\n",
        "    print(f\"⚠️ Eficiencia inferior al benchmark ({benchmark} kWh/unidad)\")\n",
        "\n",
        "# Recomendaciones\n",
        "print(\"\\nRECOMENDACIONES:\")\n",
        "\n",
        "if len(peaks) > 1:\n",
        "    print(\"• Investigar causas de los diferentes regímenes de consumo\")\n",
        "    print(\"  (cambios de turno, tipo de producto, mantenimiento)\")\n",
        "\n",
        "if skewness > 1:\n",
        "    print(\"• Atención a picos de consumo (cola derecha larga)\")\n",
        "    print(\"  Implementar medidas para reducir consumos extremos\")\n",
        "\n",
        "if df[energy_col].std() > mean_energy * 0.3:\n",
        "    print(f\"• Alta variabilidad (CV > 30%) - estandarizar operaciones\")\n",
        "\n",
        "# Eficiencia por línea\n",
        "linea_menos_eficiente = None\n",
        "max_consumo = 0\n",
        "for linea in df[production_line_col].unique():\n",
        "    consumo_linea = df[df[production_line_col] == linea][energy_col].mean()\n",
        "    if consumo_linea > max_consumo:\n",
        "        max_consumo = consumo_linea\n",
        "        linea_menos_eficiente = linea\n",
        "\n",
        "if linea_menos_eficiente:\n",
        "    print(f\"• Revisar eficiencia en {linea_menos_eficiente}\")\n",
        "    print(\"  Implementar prácticas de ahorro energético\")\n",
        "\n",
        "# Oportunidades de ahorro\n",
        "potencial_ahorro = (df[energy_col].quantile(0.9) - df[energy_col].median()) * len(df) * 0.1\n",
        "if potencial_ahorro > 1000:\n",
        "    print(f\"• Potencial de ahorro estimado: {potencial_ahorro:.0f} kWh\")\n",
        "    print(\"  Enfocar en reducir los consumos más altos\")\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)"
