        "# ============================================================\n",
        "# FIGURA 10: Heatmap de correlación\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"GENERANDO FIGURA 10\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Seleccionar variables numéricas para correlación\n",
        "variables_numericas = [\n",
        "    units_col,              # Units Produced\n",
        "    downtime_col,           # Downtime Minutes\n",
        "    energy_col,             # Energy Consumption\n",
        "    temp_col,               # Temperature C\n",
        "    defect_rate_col,        # Defect Rate\n",
        "    cost_col,               # Cost per Unit\n",
        "    revenue_col             # Revenue USD\n",
        "]\n",
        "\n",
        "# Crear DataFrame solo con variables numéricas\n",
        "df_corr = df[variables_numericas].copy()\n",
        "\n",
        "# Renombrar para mejor visualización\n",
        "nombres_mostrar = {\n",
        "    units_col: 'Units\\nProduced',\n",
        "    downtime_col: 'Downtime\\nMinutes',\n",
        "    energy_col: 'Energy\\nConsumption',\n",
        "    temp_col: 'Temperature\\nC',\n",
        "    defect_rate_col: 'Defect\\nRate',\n",
        "    cost_col: 'Cost\\nper Unit',\n",
        "    revenue_col: 'Revenue\\nUSD'\n",
        "}\n",
        "\n",
        "df_corr_renombrado = df_corr.rename(columns=nombres_mostrar)\n",
        "\n",
        "# Calcular matriz de correlación\n",
        "corr_matrix = df_corr_renombrado.corr()\n",
        "\n",
        "# Crear figura\n",
        "fig, ax = plt.subplots(figsize=(10, 8))\n",
        "\n",
        "# Crear heatmap\n",
        "im = ax.imshow(corr_matrix, cmap='RdBu_r', vmin=-1, vmax=1, aspect='auto')\n",
        "\n",
        "# Configurar ejes\n",
        "ax.set_xticks(range(len(corr_matrix.columns)))\n",
        "ax.set_yticks(range(len(corr_matrix.columns)))\n",
        "ax.set_xticklabels(corr_matrix.columns, rotation=45, ha='right', fontsize=10)\n",
        "ax.set_yticklabels(corr_matrix.columns, fontsize=10)\n",
        "\n",
        "# Añadir valores en las celdas\n",
        "for i in range(len(corr_matrix.columns)):\n",
        "    for j in range(len(corr_matrix.columns)):\n",
        "        valor = corr_matrix.iloc[i, j]\n",
        "        # Determinar color del texto (blanco para valores extremos, negro para intermedios)\n",
        "        color_texto = 'white' if abs(valor) > 0.5 else 'black'\n",
        "        ax.text(j, i, f'{valor:.2f}', ha='center', va='center', color=color_texto, fontsize=9)\n",
        "\n",
        "# Título\n",
        "ax.set_title('Fig. 10: Heatmap de correlación', fontsize=13, fontweight='bold', pad=20)\n",
        "\n",
        "# Añadir colorbar\n",
        "cbar = plt.colorbar(im, ax=ax, shrink=0.8)\n",
        "cbar.set_label('Correlación (r)', fontsize=11)\n",
        "\n",
        "# Ajustar layout\n",
        "plt.tight_layout()\n",
        "\n",
        "# Mostrar figura\n",
        "plt.show()\n",
        "\n",
        "# ============================================================\n",
        "# INTERPRETACIÓN DE LA FIGURA 10\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"INTERPRETACIÓN FIGURA 10\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# ============================================================\n",
        "# 1. MATRIZ DE CORRELACIÓN COMPLETA\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n1. MATRIZ DE CORRELACIÓN:\")\n",
        "print(\"-\" * 60)\n",
        "print(corr_matrix.round(3))\n",
        "\n",
        "# ============================================================\n",
        "# 2. CORRELACIONES FUERTES (|r| > 0.7)\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"2. CORRELACIONES FUERTES (|r| > 0.7)\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "correlaciones_fuertes = []\n",
        "for i in range(len(corr_matrix.columns)):\n",
        "    for j in range(i+1, len(corr_matrix.columns)):\n",
        "        var1 = corr_matrix.columns[i]\n",
        "        var2 = corr_matrix.columns[j]\n",
        "        valor = corr_matrix.iloc[i, j]\n",
        "        if abs(valor) > 0.7:\n",
        "            correlaciones_fuertes.append((var1, var2, valor))\n",
        "            if valor > 0:\n",
        "                print(f\"\\n{var1} — {var2}: r = {valor:.3f} (correlación POSITIVA fuerte)\")\n",
        "            else:\n",
        "                print(f\"\\n{var1} — {var2}: r = {valor:.3f} (correlación NEGATIVA fuerte)\")\n",
        "\n",
        "if not correlaciones_fuertes:\n",
        "    print(\"\\nNo hay correlaciones fuertes (|r| > 0.7)\")\n",
        "\n",
        "# ============================================================\n",
        "# 3. CORRELACIONES MODERADAS (0.5 < |r| ≤ 0.7)\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"3. CORRELACIONES MODERADAS (0.5 < |r| ≤ 0.7)\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "correlaciones_moderadas = []\n",
        "for i in range(len(corr_matrix.columns)):\n",
        "    for j in range(i+1, len(corr_matrix.columns)):\n",
        "        var1 = corr_matrix.columns[i]\n",
        "        var2 = corr_matrix.columns[j]\n",
        "        valor = corr_matrix.iloc[i, j]\n",
        "        if 0.5 < abs(valor) <= 0.7:\n",
        "            correlaciones_moderadas.append((var1, var2, valor))\n",
        "            if valor > 0:\n",
        "                print(f\"\\n{var1} — {var2}: r = {valor:.3f} (correlación POSITIVA moderada)\")\n",
        "            else:\n",
        "                print(f\"\\n{var1} — {var2}: r = {valor:.3f} (correlación NEGATIVA moderada)\")\n",
        "\n",
        "if not correlaciones_moderadas:\n",
        "    print(\"\\nNo hay correlaciones moderadas (0.5 < |r| ≤ 0.7)\")\n",
        "\n",
        "# ============================================================\n",
        "# 4. ANÁLISIS POR VARIABLE CLAVE\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"4. ANÁLISIS POR VARIABLE CLAVE\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# 4.1 Correlaciones con Units Produced\n",
        "print(\"\\n4.1 UNITS PRODUCED - Correlaciones:\")\n",
        "var_target = nombres_mostrar[units_col]\n",
        "for var in corr_matrix.columns:\n",
        "    if var != var_target:\n",
        "        valor = corr_matrix.loc[var_target, var]\n",
        "        if abs(valor) > 0.3:\n",
        "            if valor > 0:\n",
        "                print(f\"  {var}: r = {valor:.3f} (positiva)\")\n",
        "            else:\n",
        "                print(f\"  {var}: r = {valor:.3f} (negativa)\")\n",
        "\n",
        "# 4.2 Correlaciones con Defect Rate\n",
        "print(\"\\n4.2 DEFECT RATE - Correlaciones:\")\n",
        "var_target = nombres_mostrar[defect_rate_col]\n",
        "for var in corr_matrix.columns:\n",
        "    if var != var_target:\n",
        "        valor = corr_matrix.loc[var_target, var]\n",
        "        if abs(valor) > 0.3:\n",
        "            if valor > 0:\n",
        "                print(f\"  {var}: r = {valor:.3f} (positiva)\")\n",
        "            else:\n",
        "                print(f\"  {var}: r = {valor:.3f} (negativa)\")\n",
        "\n",
        "# 4.3 Correlaciones con Revenue\n",
        "print(\"\\n4.3 REVENUE - Correlaciones:\")\n",
        "var_target = nombres_mostrar[revenue_col]\n",
        "for var in corr_matrix.columns:\n",
        "    if var != var_target:\n",
        "        valor = corr_matrix.loc[var_target, var]\n",
        "        if abs(valor) > 0.3:\n",
        "            if valor > 0:\n",
        "                print(f\"  {var}: r = {valor:.3f} (positiva)\")\n",
        "            else:\n",
        "                print(f\"  {var}: r = {valor:.3f} (negativa)\")\n",
        "\n",
        "# ============================================================\n",
        "# 5. INTERPRETACIÓN DE RELACIONES CLAVE\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"5. INTERPRETACIÓN DE RELACIONES CLAVE\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# 5.1 Relación Producción - Ingresos\n",
        "prod_rev = corr_matrix.loc[nombres_mostrar[units_col], nombres_mostrar[revenue_col]]\n",
        "print(f\"\\n5.1 Producción vs Ingresos: r = {prod_rev:.3f}\")\n",
        "if prod_rev > 0.8:\n",
        "    print(\"  → Relación MUY FUERTE: más producción genera más ingresos\")\n",
        "    print(\"    (relación casi lineal, esperable en producción)\")\n",
        "elif prod_rev > 0.6:\n",
        "    print(\"  → Relación FUERTE: hay clara asociación positiva\")\n",
        "    print(\"    (a mayor producción, mayores ingresos)\")\n",
        "elif prod_rev > 0.4:\n",
        "    print(\"  → Relación MODERADA: producción explica parte de los ingresos\")\n",
        "    print(\"    (otros factores como precio o mezcla de productos influyen)\")\n",
        "else:\n",
        "    print(\"  → Relación DÉBIL: producción no es el principal driver de ingresos\")\n",
        "    print(\"    (precios, eficiencia o tipo de producto son más importantes)\")\n",
        "\n",
        "# 5.2 Relación Producción - Defectos\n",
        "prod_def = corr_matrix.loc[nombres_mostrar[units_col], nombres_mostrar[defect_rate_col]]\n",
        "print(f\"\\n5.2 Producción vs Defectos: r = {prod_def:.3f}\")\n",
        "if prod_def > 0.5:\n",
        "    print(\"  → ALERTA: producir más aumenta significativamente los defectos\")\n",
        "    print(\"    (posible saturación de línea o control de calidad insuficiente)\")\n",
        "elif prod_def > 0.3:\n",
        "    print(\"  → Preocupación moderada: hay trade-off volumen-calidad\")\n",
        "    print(\"    (revisar puntos de equilibrio)\")\n",
        "elif prod_def < -0.3:\n",
        "    print(\"  → Interesante: más producción, menos defectos\")\n",
        "    print(\"    (efecto aprendizaje o lotes más grandes mejoran calidad)\")\n",
        "else:\n",
        "    print(\"  → No hay relación clara: volumen no afecta calidad\")\n",
        "    print(\"    (proceso estable y controlado)\")\n",
        "\n",
        "# 5.3 Relación Energía - Producción\n",
        "energy_prod = corr_matrix.loc[nombres_mostrar[energy_col], nombres_mostrar[units_col]]\n",
        "print(f\"\\n5.3 Energía vs Producción: r = {energy_prod:.3f}\")\n",
        "if energy_prod > 0.8:\n",
        "    print(\"  → Relación MUY FUERTE: consumo energético directamente ligado a volumen\")\n",
        "    print(\"    (eficiencia energética constante)\")\n",
        "elif energy_prod > 0.6:\n",
        "    print(\"  → Relación FUERTE: a más producción, más energía\")\n",
        "    print(\"    (relación esperable pero con cierta variabilidad)\")\n",
        "elif energy_prod > 0.4:\n",
        "    print(\"  → Relación MODERADA: otros factores afectan consumo energético\")\n",
        "    print(\"    (tipo de producto, mantenimiento, etc.)\")\n",
        "else:\n",
        "    print(\"  → Relación DÉBIL: consumo energético no depende del volumen\")\n",
        "    print(\"    (equipos siempre encendidos, producción no es el driver principal)\")\n",
        "\n",
        "# 5.4 Relación Energía - Defectos\n",
        "energy_def = corr_matrix.loc[nombres_mostrar[energy_col], nombres_mostrar[defect_rate_col]]\n",
        "print(f\"\\n5.4 Energía vs Defectos: r = {energy_def:.3f}\")\n",
        "if energy_def > 0.4:\n",
        "    print(\"  → Correlación positiva: más energía, más defectos\")\n",
        "    print(\"    (posible sobre-procesamiento o estrés de equipos)\")\n",
        "elif energy_def < -0.4:\n",
        "    print(\"  → Correlación negativa: más energía, menos defectos\")\n",
        "    print(\"    (mayor energía asociada a mejor control de proceso)\")\n",
        "else:\n",
        "    print(\"  → Sin relación clara: consumo energético no afecta calidad\")\n",
        "\n",
        "# 5.5 Relación Downtime - Defectos\n",
        "downtime_def = corr_matrix.loc[nombres_mostrar[downtime_col], nombres_mostrar[defect_rate_col]]\n",
        "print(f\"\\n5.5 Downtime vs Defectos: r = {downtime_def:.3f}\")\n",
        "if downtime_def > 0.3:\n",
        "    print(\"  → Correlación positiva: más paros, más defectos\")\n",
        "    print(\"    (paradas generan inestabilidad en el proceso)\")\n",
        "elif downtime_def < -0.3:\n",
        "    print(\"  → Correlación negativa: más paros, menos defectos\")\n",
        "    print(\"    (paradas permiten ajustes o mantenimiento que mejora calidad)\")\n",
        "else:\n",
        "    print(\"  → Sin relación: los paros no afectan la calidad directamente\")\n",
        "\n",
        "# 5.6 Relación Temperatura - Defectos\n",
        "temp_def = corr_matrix.loc[nombres_mostrar[temp_col], nombres_mostrar[defect_rate_col]]\n",
        "print(f\"\\n5.6 Temperatura vs Defectos: r = {temp_def:.3f}\")\n",
        "if abs(temp_def) > 0.3:\n",
        "    if temp_def > 0:\n",
        "        print(\"  → Temperaturas más altas asociadas a más defectos\")\n",
        "        print(\"    (revisar control térmico del proceso)\")\n",
        "    else:\n",
        "        print(\"  → Temperaturas más altas asociadas a menos defectos\")\n",
        "        print(\"    (el proceso funciona mejor en rangos más cálidos)\")\n",
        "else:\n",
        "    print(\"  → Sin relación: temperatura dentro de rangos de control\")\n",
        "\n",
        "# 5.7 Relación Costo - Defectos\n",
        "cost_def = corr_matrix.loc[nombres_mostrar[cost_col], nombres_mostrar[defect_rate_col]]\n",
        "print(f\"\\n5.7 Costo vs Defectos: r = {cost_def:.3f}\")\n",
        "if cost_def > 0.4:\n",
        "    print(\"  → Correlación positiva: más defectos, mayor costo\")\n",
        "    print(\"    (costos de mala calidad: reproceso, scrap, inspección)\")\n",
        "elif cost_def < -0.4:\n",
        "    print(\"  → Correlación negativa: más defectos, menor costo\")\n",
        "    print(\"    (posible ahorro en controles de calidad o materiales)\")\n",
        "else:\n",
        "    print(\"  → Sin relación clara: los defectos no impactan el costo unitario\")\n",
        "\n",
        "# ============================================================\n",
        "# 6. RESUMEN DE HALLAZGOS\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"6. RESUMEN DE HALLAZGOS\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Identificar las 3 correlaciones más fuertes (positivas)\n",
        "print(\"\\nCORRELACIONES POSITIVAS MÁS FUERTES:\")\n",
        "corr_pares = []\n",
        "for i in range(len(corr_matrix.columns)):\n",
        "    for j in range(i+1, len(corr_matrix.columns)):\n",
        "        corr_pares.append((corr_matrix.columns[i], corr_matrix.columns[j], corr_matrix.iloc[i, j]))\n",
        "\n",
        "corr_pares.sort(key=lambda x: abs(x[2]), reverse=True)\n",
        "\n",
        "positivas = [p for p in corr_pares if p[2] > 0]\n",
        "for i, (var1, var2, valor) in enumerate(positivas[:3]):\n",
        "    print(f\"{i+1}. {var1} — {var2}: r = {valor:.3f}\")\n",
        "\n",
        "# Identificar las 3 correlaciones negativas más fuertes\n",
        "print(\"\\nCORRELACIONES NEGATIVAS MÁS FUERTES:\")\n",
        "negativas = [p for p in corr_pares if p[2] < 0]\n",
        "for i, (var1, var2, valor) in enumerate(negativas[:3]):\n",
        "    print(f\"{i+1}. {var1} — {var2}: r = {valor:.3f}\")\n",
        "\n",
        "# ============================================================\n",
        "# 7. RECOMENDACIONES BASADAS EN CORRELACIONES\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"7. RECOMENDACIONES\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "recomendaciones = []\n",
        "\n",
        "# Basado en producción-defectos\n",
        "if prod_def > 0.4:\n",
        "    recomendaciones.append(\"• Revisar capacidad de líneas: aumentar producción está degradando calidad\")\n",
        "    recomendaciones.append(\"• Implementar pausas o rotación para evitar fatiga de equipos\")\n",
        "\n",
        "# Basado en energía-defectos\n",
        "if energy_def > 0.3:\n",
        "    recomendaciones.append(\"• Investigar por qué alto consumo energético genera defectos\")\n",
        "    recomendaciones.append(\"• Posible sobrecalentamiento o estrés de materiales\")\n",
        "\n",
        "# Basado en downtime-defectos\n",
        "if downtime_def > 0.3:\n",
        "    recomendaciones.append(\"• Reducir paradas no programadas: afectan la estabilidad del proceso\")\n",
        "    recomendaciones.append(\"• Implementar mantenimiento preventivo en lugar de correctivo\")\n",
        "\n",
        "# Basado en temperatura-defectos\n",
        "if abs(temp_def) > 0.3:\n",
        "    recomendaciones.append(\"• Controlar temperatura: afecta significativamente la calidad\")\n",
        "    recomendaciones.append(\"• Revisar sistemas de climatización por turno\")\n",
        "\n",
        "# Basado en costo-defectos\n",
        "if cost_def > 0.3:\n",
        "    recomendaciones.append(\"• Invertir en prevención de defectos: reducirá costos unitarios\")\n",
        "    recomendaciones.append(\"• Análisis de costo-beneficio de mejoras de calidad\")\n",
        "\n",
        "# Si hay pocas correlaciones fuertes\n",
        "if len(correlaciones_fuertes) < 2:\n",
        "    recomendaciones.append(\"• Variables relativamente independientes: proceso estable\")\n",
        "    recomendaciones.append(\"• Enfocar mejoras en cada variable por separado\")\n",
        "\n",
        "# Mostrar recomendaciones\n",
        "if recomendaciones:\n",
        "    print(\"\\n\" + \"\\n\".join(recomendaciones[:5]))  # Mostrar máximo 5\n",
        "else:\n",
        "    print(\"\\nNo hay correlaciones suficientemente fuertes para generar recomendaciones específicas.\")\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"NOTA: Correlación no implica causalidad\")\n",
        "print(\"Las relaciones observadas deben investigarse para entender causas reales\")\n",
        "print(\"=\"*60)"
