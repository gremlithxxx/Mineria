      "source": [
        "# ============================================================\n",
        "# PRIMERA PARTE: CARGA Y PREPARACIÓN DE DATOS DESDE GOOGLE DRIVE\n",
        "# ============================================================\n",
        "\n",
        "# Importar librerías necesarias\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "from scipy import stats\n",
        "import warnings\n",
        "warnings.filterwarnings('ignore')\n",
        "\n",
        "# ============================================================\n",
        "# 1. CONFIGURACIÓN INICIAL\n",
        "# ============================================================\n",
        "\n",
        "# Configurar estilo de gráficos\n",
        "plt.style.use('default')\n",
        "sns.set_style(\"whitegrid\")\n",
        "\n",
        "# Colores y formas para líneas de producción\n",
        "colors_lines = {\n",
        "    'Line_A': '#1f77b4',\n",
        "    'Line_B': '#ff7f0e',\n",
        "    'Line_C': '#2ca02c'\n",
        "}\n",
        "\n",
        "markers_lines = {\n",
        "    'Line_A': 'o',\n",
        "    'Line_B': 's',\n",
        "    'Line_C': '^'\n",
        "}\n",
        "\n",
        "# Colores y estilos para turnos\n",
        "colors_shift = {\n",
        "    'Morning': '#9467bd',\n",
        "    'Evening': '#8c564b',\n",
        "    'Night': '#d62728'\n",
        "}\n",
        "\n",
        "line_styles_shift = {\n",
        "    'Morning': '-',\n",
        "    'Evening': '--',\n",
        "    'Night': '-.'\n",
        "}\n",
        "\n",
        "# ============================================================\n",
        "# 2. MONTAJE DE GOOGLE DRIVE\n",
        "# ============================================================\n",
        "\n",
        "print(\"=\"*60)\n",
        "print(\"CARGA DE DATOS - INDUSTRIA\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "print(\"\\n[1/5] Montando Google Drive...\")\n",
        "\n",
        "from google.colab import drive\n",
        "drive.mount('/content/drive')\n",
        "\n",
        "# ============================================================\n",
        "# 3. UBICACIÓN DEL ARCHIVO EN DRIVE\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n[2/5] Buscando archivo en Drive...\")\n",
        "\n",
        "# Ruta base en Drive\n",
        "base_path = '/content/drive/MyDrive/base/'\n",
        "file_name = 'base.xlsx.pdf'  # Ajusta la extensión según tu archivo\n",
        "\n",
        "# Intentar diferentes extensiones posibles\n",
        "import os\n",
        "possible_files = ['base.xlsx', 'base.pdf', 'base.xlsx.pdf', 'base.csv', 'base.txt']\n",
        "\n",
        "file_path = None\n",
        "for ext_file in possible_files:\n",
        "    test_path = os.path.join(base_path, ext_file)\n",
        "    if os.path.exists(test_path):\n",
        "        file_path = test_path\n",
        "        print(f\"  Archivo encontrado: {ext_file}\")\n",
        "        break\n",
        "\n",
        "if file_path is None:\n",
        "    print(\"  No se encontró el archivo. Mostrando contenido de la carpeta:\")\n",
        "    if os.path.exists(base_path):\n",
        "        print(os.listdir(base_path))\n",
        "    else:\n",
        "        print(f\"  La carpeta {base_path} no existe\")\n",
        "        # Crear carpeta si no existe\n",
        "        os.makedirs(base_path, exist_ok=True)\n",
        "        print(f\"  Carpeta creada: {base_path}\")\n",
        "        print(\"  Por favor, sube tu archivo 'base' a esta carpeta\")\n",
        "else:\n",
        "    print(f\"  Ruta completa: {file_path}\")\n",
        "\n",
        "# ============================================================\n",
        "# 4. FUNCIÓN PARA EXTRAER DATOS\n",
        "# ============================================================\n",
        "\n",
        "def extract_data_from_file(file_path):\n",
        "    \"\"\"\n",
        "    Extrae los datos del archivo\n",
        "    \"\"\"\n",
        "\n",
        "    column_names = [\n",
        "        'Product_ID', 'Production_Line', 'Shift', 'Units_Produced',\n",
        "        'Downtime_Minutes', 'Energy_Consumption_kWh', 'Temperature_C',\n",
        "        'Defect_Count', 'Inspected_Units', 'Defect_Rate', 'Cost_per_Unit',\n",
        "        'Revenue_USD', 'Batch'\n",
        "    ]\n",
        "\n",
        "    data_rows = []\n",
        "\n",
        "    try:\n",
        "        # Intentar leer como texto plano\n",
        "        with open(file_path, 'r', encoding='utf-8', errors='ignore') as file:\n",
        "            lines = file.readlines()\n",
        "\n",
        "        for line in lines:\n",
        "            parts = line.strip().split()\n",
        "\n",
        "            if parts and parts[0].replace('.', '').replace(',', '').isdigit():\n",
        "                if len(parts) >= 13:\n",
        "                    cleaned_parts = []\n",
        "                    for p in parts[:13]:\n",
        "                        if isinstance(p, str) and p.startswith('0') and '.' in p and len(p) > 1:\n",
        "                            p = p[1:]\n",
        "                        cleaned_parts.append(p)\n",
        "\n",
        "                    data_rows.append(cleaned_parts)\n",
        "\n",
        "        if data_rows:\n",
        "            df = pd.DataFrame(data_rows, columns=column_names)\n",
        "            return df\n",
        "        else:\n",
        "            print(\"  No se encontraron datos en el formato esperado\")\n",
        "\n",
        "            # Intentar leer con pandas\n",
        "            try:\n",
        "                if file_path.endswith('.csv'):\n",
        "                    df = pd.read_csv(file_path)\n",
        "                elif file_path.endswith('.xlsx'):\n",
        "                    df = pd.read_excel(file_path)\n",
        "                else:\n",
        "                    df = pd.read_csv(file_path, sep='\\s+')\n",
        "                return df\n",
        "            except:\n",
        "                return None\n",
        "\n",
        "    except Exception as e:\n",
        "        print(f\"  Error al leer archivo: {e}\")\n",
        "        return None\n",
        "\n",
        "# ============================================================\n",
        "# 5. CARGAR DATOS\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n[3/5] Cargando datos...\")\n",
        "\n",
        "df = None\n",
        "if file_path and os.path.exists(file_path):\n",
        "    df = extract_data_from_file(file_path)\n",
        "\n",
        "# Si no se pudo cargar, crear datos mínimos de ejemplo\n",
        "if df is None or len(df) == 0:\n",
        "    print(\"  Usando datos de ejemplo incluidos\")\n",
        "\n",
        "    data_example = [\n",
        "        [1, 'Line_A', 'Morning', 253, 32, 120.2, 22, 22, 25, 25.9, 4.94, 3943.18, 1],\n",
        "        [2, 'Line_A', 'Morning', 234, 35, 108.0, 24, 29, 26, 26.2, 5.08, 3640.75, 1],\n",
        "        [3, 'Line_A', 'Morning', 196, 34, 88.0, 21, 34, 23, 23.7, 4.59, 2727.02, 1],\n",
        "        [13, 'Line_B', 'Morning', 300, 54, 136.6, 27, 21, 25, 25.2, 5.15, 4575.35, 1],\n",
        "        [25, 'Line_C', 'Morning', 274, 72, 132.0, 22, 31, 26, 26.6, 4.81, 3857.5, 1]\n",
        "    ]\n",
        "\n",
        "    column_names = [\n",
        "        'Product_ID', 'Production_Line', 'Shift', 'Units_Produced',\n",
        "        'Downtime_Minutes', 'Energy_Consumption_kWh', 'Temperature_C',\n",
        "        'Defect_Count', 'Inspected_Units', 'Defect_Rate', 'Cost_per_Unit',\n",
        "        'Revenue_USD', 'Batch'\n",
        "    ]\n",
        "\n",
        "    df = pd.DataFrame(data_example, columns=column_names)\n",
        "    print(\"  Datos de ejemplo cargados\")\n",
        "\n",
        "# ============================================================\n",
        "# 6. LIMPIEZA DE DATOS\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n[4/5] Limpiando datos...\")\n",
        "\n",
        "numeric_columns = [\n",
        "    'Units_Produced', 'Downtime_Minutes', 'Energy_Consumption_kWh',\n",
        "    'Temperature_C', 'Defect_Count', 'Inspected_Units', 'Defect_Rate',\n",
        "    'Cost_per_Unit', 'Revenue_USD', 'Batch'\n",
        "]\n",
        "\n",
        "for col in numeric_columns:\n",
        "    if col in df.columns:\n",
        "        df[col] = pd.to_numeric(df[col], errors='coerce')\n",
        "\n",
        "df['Production_Line'] = df['Production_Line'].astype(str).str.strip()\n",
        "df['Shift'] = df['Shift'].astype(str).str.strip()\n",
        "\n",
        "# Calcular tasa de defectos si es necesario\n",
        "if 'Defect_Rate' in df.columns and 'Defect_Count' in df.columns and 'Inspected_Units' in df.columns:\n",
        "    df['Defect_Rate_calc'] = (df['Defect_Count'] / df['Inspected_Units']) * 100\n",
        "    df['Defect_Rate_final'] = df['Defect_Rate'].fillna(df['Defect_Rate_calc'])\n",
        "else:\n",
        "    df['Defect_Rate_final'] = df.get('Defect_Rate', np.nan)\n",
        "\n",
        "# Crear Batch si no existe\n",
        "if 'Batch' not in df.columns or df['Batch'].isna().all():\n",
        "    if 'Product_ID' in df.columns:\n",
        "        df['Batch'] = ((df['Product_ID'] - 1) // 20) + 1\n",
        "    else:\n",
        "        df['Batch'] = 1\n",
        "\n",
        "# ============================================================\n",
        "# 7. VERIFICACIÓN FINAL\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n[5/5] Resumen de datos cargados:\")\n",
        "print(f\"  Registros: {len(df)}\")\n",
        "print(f\"  Líneas: {df['Production_Line'].unique().tolist()}\")\n",
        "print(f\"  Turnos: {df['Shift'].unique().tolist()}\")\n",
        "print(f\"  Batches: {df['Batch'].nunique()}\")\n",
        "print(f\"  Product IDs: {df['Product_ID'].min()} - {df['Product_ID'].max()}\")\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"CARGA COMPLETADA - Datos listos\")\n",
        "print(\"=\"*60)\n",
        "print(\"\\nPrimeras 3 filas:\")\n",
        "print(df.head(3))\n",
        "\n",
        "# ============================================================\n",
        "# 8. VARIABLES PARA GRÁFICOS\n",
        "# ============================================================\n",
        "\n",
        "production_line_col = 'Production_Line'\n",
        "shift_col = 'Shift'\n",
        "units_col = 'Units_Produced'\n",
        "downtime_col = 'Downtime_Minutes'\n",
        "energy_col = 'Energy_Consumption_kWh'\n",
        "temp_col = 'Temperature_C'\n",
        "defect_rate_col = 'Defect_Rate_final'\n",
        "cost_col = 'Cost_per_Unit'\n",
        "revenue_col = 'Revenue_USD'\n",
        "batch_col = 'Batch'\n",
        "product_id_col = 'Product_ID'\n",
        "\n",
        "print(\"\\nColumnas disponibles:\")\n",
        "print(f\"  - Producción: {units_col}\")\n",
        "print(f\"  - Defectos: {defect_rate_col}\")\n",
        "print(f\"  - Ingresos: {revenue_col}\")\n",
        "print(f\"  - Energía: {energy_col}\")\n",
        "print(f\"  - Temperatura: {temp_col}\")\n",
        "print(f\"  - Costo: {cost_col}\")"
      ]
