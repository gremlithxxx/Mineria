        "# ============================================================\n",
        "# FIGURA 6: Ingresos promedio por turno (±EE)\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"GENERANDO FIGURA 6\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Crear la figura\n",
        "fig, ax = plt.subplots(figsize=(10, 6))\n",
        "\n",
        "# Calcular estadísticas por turno\n",
        "stats_turnos = df.groupby(shift_col)[revenue_col].agg(['mean', 'std', 'count']).reset_index()\n",
        "stats_turnos.columns = [shift_col, 'media', 'std', 'count']\n",
        "\n",
        "# Calcular error estándar (EE = std / sqrt(n))\n",
        "stats_turnos['ee'] = stats_turnos['std'] / np.sqrt(stats_turnos['count'])\n",
        "\n",
        "# Orden de turnos (Morning, Evening, Night)\n",
        "orden_turnos = ['Morning', 'Evening', 'Night']\n",
        "stats_turnos['orden'] = stats_turnos[shift_col].map({v: i for i, v in enumerate(orden_turnos)})\n",
        "stats_turnos = stats_turnos.sort_values('orden')\n",
        "\n",
        "# Colores para las barras (usando los colores de turno)\n",
        "bar_colors = [colors_shift[shift] for shift in stats_turnos[shift_col]]\n",
        "\n",
        "# Crear barras\n",
        "bars = ax.bar(\n",
        "    stats_turnos[shift_col],\n",
        "    stats_turnos['media'],\n",
        "    yerr=stats_turnos['ee'],\n",
        "    capsize=5,\n",
        "    color=bar_colors,\n",
        "    edgecolor='black',\n",
        "    linewidth=1,\n",
        "    alpha=0.8,\n",
        "    error_kw={'elinewidth': 1.5, 'ecolor': 'black', 'capthick': 1.5}\n",
        ")\n",
        "\n",
        "# Añadir etiquetas numéricas encima de cada barra\n",
        "for i, (idx, row) in enumerate(stats_turnos.iterrows()):\n",
        "    ax.text(\n",
        "        i,\n",
        "        row['media'] + row['ee'] + 50,  # Posición encima de la barra de error\n",
        "        f'${row[\"media\"]:,.0f}',\n",
        "        ha='center',\n",
        "        va='bottom',\n",
        "        fontsize=10,\n",
        "        fontweight='bold'\n",
        "    )\n",
        "\n",
        "# Configurar ejes\n",
        "ax.set_xlabel('Shift', fontsize=11)\n",
        "ax.set_ylabel('Revenue USD (promedio)', fontsize=11)\n",
        "\n",
        "# Configurar ticks\n",
        "ax.yaxis.set_major_formatter(plt.FuncFormatter(lambda y, _: f'${int(y):,}'))\n",
        "\n",
        "# Configurar cuadrícula (solo eje Y)\n",
        "ax.grid(True, linestyle='--', linewidth=0.7, alpha=0.6, axis='y')\n",
        "\n",
        "# Configurar bordes\n",
        "for spine in ax.spines.values():\n",
        "    spine.set_color('#222')\n",
        "    spine.set_linewidth(1.2)\n",
        "\n",
        "# Título\n",
        "ax.set_title('Fig. 6: Ingresos promedio por turno (±EE)', fontsize=13, fontweight='bold', pad=15)\n",
        "\n",
        "# Leyenda para error\n",
        "from matplotlib.patches import Patch\n",
        "from matplotlib.lines import Line2D\n",
        "legend_elements = [\n",
        "    Patch(facecolor='gray', alpha=0.5, label='Promedio'),\n",
        "    Line2D([0], [0], color='black', linewidth=1.5, label='± Error Estándar')\n",
        "]\n",
        "ax.legend(handles=legend_elements, loc='upper right', frameon=True)\n",
        "\n",
        "# Ajustar layout\n",
        "plt.tight_layout()\n",
        "\n",
        "# Mostrar figura\n",
        "plt.show()\n",
        "\n",
        "# ============================================================\n",
        "# FIGURA 6B: Ingresos normalizados por unidad producida\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"GENERANDO FIGURA 6B - Ingresos normalizados\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Crear figura para ingresos normalizados\n",
        "fig2, ax2 = plt.subplots(figsize=(10, 6))\n",
        "\n",
        "# Calcular ingresos por unidad\n",
        "df['Revenue_per_Unit'] = df[revenue_col] / df[units_col]\n",
        "\n",
        "# Calcular estadísticas por turno\n",
        "stats_turnos_norm = df.groupby(shift_col)['Revenue_per_Unit'].agg(['mean', 'std', 'count']).reset_index()\n",
        "stats_turnos_norm.columns = [shift_col, 'media', 'std', 'count']\n",
        "stats_turnos_norm['ee'] = stats_turnos_norm['std'] / np.sqrt(stats_turnos_norm['count'])\n",
        "\n",
        "# Ordenar\n",
        "stats_turnos_norm['orden'] = stats_turnos_norm[shift_col].map({v: i for i, v in enumerate(orden_turnos)})\n",
        "stats_turnos_norm = stats_turnos_norm.sort_values('orden')\n",
        "\n",
        "# Crear barras normalizadas\n",
        "bars2 = ax2.bar(\n",
        "    stats_turnos_norm[shift_col],\n",
        "    stats_turnos_norm['media'],\n",
        "    yerr=stats_turnos_norm['ee'],\n",
        "    capsize=5,\n",
        "    color=bar_colors,\n",
        "    edgecolor='black',\n",
        "    linewidth=1,\n",
        "    alpha=0.8,\n",
        "    error_kw={'elinewidth': 1.5, 'ecolor': 'black', 'capthick': 1.5}\n",
        ")\n",
        "\n",
        "# Añadir etiquetas\n",
        "for i, (idx, row) in enumerate(stats_turnos_norm.iterrows()):\n",
        "    ax2.text(\n",
        "        i,\n",
        "        row['media'] + row['ee'] + 0.5,\n",
        "        f'${row[\"media\"]:.2f}',\n",
        "        ha='center',\n",
        "        va='bottom',\n",
        "        fontsize=10,\n",
        "        fontweight='bold'\n",
        "    )\n",
        "\n",
        "# Configurar ejes\n",
        "ax2.set_xlabel('Shift', fontsize=11)\n",
        "ax2.set_ylabel('Revenue por Unidad (USD)', fontsize=11)\n",
        "ax2.yaxis.set_major_formatter(plt.FuncFormatter(lambda y, _: f'${y:.2f}'))\n",
        "ax2.grid(True, linestyle='--', linewidth=0.7, alpha=0.6, axis='y')\n",
        "\n",
        "# Configurar bordes\n",
        "for spine in ax2.spines.values():\n",
        "    spine.set_color('#222')\n",
        "    spine.set_linewidth(1.2)\n",
        "\n",
        "# Título\n",
        "ax2.set_title('Fig. 6b: Ingresos por unidad producida por turno (±EE)', fontsize=13, fontweight='bold', pad=15)\n",
        "ax2.legend(handles=legend_elements, loc='upper right', frameon=True)\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# ============================================================\n",
        "# INTERPRETACIÓN DE LA FIGURA 6\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"INTERPRETACIÓN FIGURA 6\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Estadísticas por turno (ingresos totales)\n",
        "print(\"\\nESTADÍSTICAS DE INGRESOS POR TURNO:\")\n",
        "print(\"-\" * 50)\n",
        "\n",
        "for idx, row in stats_turnos.iterrows():\n",
        "    print(f\"\\n{row[shift_col]}:\")\n",
        "    print(f\"  Ingreso promedio: ${row['media']:,.0f}\")\n",
        "    print(f\"  Desviación estándar: ±${row['std']:,.0f}\")\n",
        "    print(f\"  Error estándar: ±${row['ee']:,.0f}\")\n",
        "    print(f\"  Número de lotes: {int(row['count'])}\")\n",
        "    print(f\"  Rango (media ± EE): ${row['media'] - row['ee']:,.0f} - ${row['media'] + row['ee']:,.0f}\")\n",
        "\n",
        "# Estadísticas por turno (ingresos normalizados)\n",
        "print(\"\\n\" + \"=\"*50)\n",
        "print(\"ESTADÍSTICAS DE INGRESOS POR UNIDAD:\")\n",
        "print(\"-\" * 50)\n",
        "\n",
        "for idx, row in stats_turnos_norm.iterrows():\n",
        "    print(f\"\\n{row[shift_col]}:\")\n",
        "    print(f\"  Ingreso por unidad: ${row['media']:.2f}\")\n",
        "    print(f\"  Desviación estándar: ±${row['std']:.2f}\")\n",
        "    print(f\"  Error estándar: ±${row['ee']:.2f}\")\n",
        "\n",
        "# Comparación de turnos\n",
        "print(\"\\n\" + \"=\"*50)\n",
        "print(\"COMPARACIÓN ENTRE TURNOS:\")\n",
        "print(\"-\" * 50)\n",
        "\n",
        "turno_max = stats_turnos.loc[stats_turnos['media'].idxmax()]\n",
        "turno_min = stats_turnos.loc[stats_turnos['media'].idxmin()]\n",
        "diferencia = turno_max['media'] - turno_min['media']\n",
        "porcentaje = (diferencia / turno_min['media']) * 100\n",
        "\n",
        "print(f\"\\nTurno con mayores ingresos: {turno_max[shift_col]} (${turno_max['media']:,.0f})\")\n",
        "print(f\"Turno con menores ingresos: {turno_min[shift_col]} (${turno_min['media']:,.0f})\")\n",
        "print(f\"Diferencia: ${diferencia:,.0f} ({porcentaje:.1f}% más)\")\n",
        "\n",
        "# Análisis de diferencias significativas\n",
        "print(\"\\n\" + \"=\"*50)\n",
        "print(\"ANÁLISIS ESTADÍSTICO (prueba t):\")\n",
        "print(\"-\" * 50)\n",
        "\n",
        "from scipy import stats\n",
        "\n",
        "for i in range(len(orden_turnos)):\n",
        "    for j in range(i+1, len(orden_turnos)):\n",
        "        turno1 = orden_turnos[i]\n",
        "        turno2 = orden_turnos[j]\n",
        "\n",
        "        data1 = df[df[shift_col] == turno1][revenue_col]\n",
        "        data2 = df[df[shift_col] == turno2][revenue_col]\n",
        "\n",
        "        t_stat, p_value = stats.ttest_ind(data1, data2)\n",
        "\n",
        "        if p_value < 0.05:\n",
        "            print(f\"{turno1} vs {turno2}: DIFERENCIA SIGNIFICATIVA (p={p_value:.4f})\")\n",
        "        else:\n",
        "            print(f\"{turno1} vs {turno2}: Diferencia no significativa (p={p_value:.4f})\")\n",
        "\n",
        "# Análisis de productividad vs ingresos\n",
        "print(\"\\n\" + \"=\"*50)\n",
        "print(\"RELACIÓN CON PRODUCTIVIDAD:\")\n",
        "print(\"-\" * 50)\n",
        "\n",
        "# Calcular producción promedio por turno\n",
        "prod_turnos = df.groupby(shift_col)[units_col].mean()\n",
        "\n",
        "for turno in orden_turnos:\n",
        "    ingreso = stats_turnos[stats_turnos[shift_col] == turno]['media'].values[0]\n",
        "    prod = prod_turnos[turno]\n",
        "    ingreso_por_unidad = ingreso / prod\n",
        "\n",
        "    print(f\"\\n{turno}:\")\n",
        "    print(f\"  Producción promedio: {prod:.0f} unidades\")\n",
        "    print(f\"  Ingreso por unidad: ${ingreso_por_unidad:.2f}\")\n",
        "\n",
        "# Análisis de variabilidad\n",
        "print(\"\\n\" + \"=\"*50)\n",
        "print(\"ANÁLISIS DE VARIABILIDAD:\")\n",
        "print(\"-\" * 50)\n",
        "\n",
        "turno_mas_variable = stats_turnos.loc[stats_turnos['std'].idxmax()]\n",
        "turno_mas_consistente = stats_turnos.loc[stats_turnos['std'].idxmin()]\n",
        "\n",
        "print(f\"\\nTurno más variable: {turno_mas_variable[shift_col]} (std=${turno_mas_variable['std']:,.0f})\")\n",
        "print(f\"Turno más consistente: {turno_mas_consistente[shift_col]} (std=${turno_mas_consistente['std']:,.0f})\")\n",
        "\n",
        "cv_turnos = (stats_turnos['std'] / stats_turnos['media']) * 100\n",
        "for idx, row in stats_turnos.iterrows():\n",
        "    cv = (row['std'] / row['media']) * 100\n",
        "    print(f\"  {row[shift_col]}: CV = {cv:.1f}%\")\n",
        "\n",
        "# ============================================================\n",
        "# INTERPRETACIÓN FINAL\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"INTERPRETACIÓN DE RESULTADOS\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# 1. Turno destacado\n",
        "print(\"\\n1. TURNO DESTACADO:\")\n",
        "if turno_max[shift_col] == 'Morning':\n",
        "    print(\"   El turno MAÑANA genera mayores ingresos totales\")\n",
        "    print(\"   Posiblemente por mayor volumen de producción o mejor mezcla de productos\")\n",
        "elif turno_max[shift_col] == 'Evening':\n",
        "    print(\"   El turno TARDE genera mayores ingresos totales\")\n",
        "    print(\"   Podría deberse a demanda pico en este horario\")\n",
        "else:\n",
        "    print(\"   El turno NOCHE genera mayores ingresos totales\")\n",
        "    print(\"   Interesante dado que normalmente se espera menor actividad nocturna\")\n",
        "\n",
        "# 2. Consistencia\n",
        "print(\"\\n2. CONSISTENCIA:\")\n",
        "if turno_mas_consistente[shift_col] == turno_max[shift_col]:\n",
        "    print(\"   El turno con mayores ingresos también es el más consistente\")\n",
        "    print(\"   Esto sugiere operaciones estables y predecibles\")\n",
        "else:\n",
        "    print(f\"   {turno_mas_consistente[shift_col]} es el más consistente, pero no el de mayores ingresos\")\n",
        "    print(\"   Podría sacrificar volumen por estabilidad\")\n",
        "\n",
        "# 3. Ingresos por unidad\n",
        "print(\"\\n3. EFICIENCIA POR UNIDAD:\")\n",
        "turno_eficiente = stats_turnos_norm.loc[stats_turnos_norm['media'].idxmax()]\n",
        "print(f\"   {turno_eficiente[shift_col]} tiene el mayor ingreso por unidad (${turno_eficiente['media']:.2f})\")\n",
        "\n",
        "if turno_eficiente[shift_col] == turno_max[shift_col]:\n",
        "    print(\"   Coincide: mayores ingresos totales y por unidad\")\n",
        "    print(\"   Este turno es claramente superior en ambos aspectos\")\n",
        "else:\n",
        "    print(f\"   {turno_max[shift_col]} gana por volumen, pero {turno_eficiente[shift_col]} es más eficiente por unidad\")\n",
        "    print(\"   Sugiere diferentes estrategias: volumen vs valor agregado\")\n",
        "\n",
        "# 4. Recomendaciones\n",
        "print(\"\\n4. RECOMENDACIONES:\")\n",
        "\n",
        "# Recomendación para turno con menores ingresos\n",
        "if turno_min[shift_col] == 'Night' and porcentaje > 20:\n",
        "    print(f\"   - Revisar operaciones nocturnas: brecha del {porcentaje:.0f}% con líder\")\n",
        "    print(\"     Posiblemente falta supervisión o condiciones diferentes\")\n",
        "\n",
        "# Recomendación sobre variabilidad\n",
        "if turno_mas_variable['std'] > turno_mas_consistente['std'] * 1.5:\n",
        "    print(f\"   - Alta variabilidad en {turno_mas_variable[shift_col]}\")\n",
        "    print(\"     Implementar controles para estabilizar ingresos\")\n",
        "\n",
        "# Recomendación sobre ingresos por unidad\n",
        "if stats_turnos_norm['media'].std() > 2:\n",
        "    print(\"   - Diferencias significativas en ingreso por unidad entre turnos\")\n",
        "    print(\"     Investigar mezcla de productos o precios por turno\")\n",
        "\n",
        "# Recomendación general\n",
        "print(\"\\n   RECOMENDACIÓN PRINCIPAL:\")\n",
        "if porcentaje > 15:\n",
        "    print(f\"   Implementar prácticas del turno {turno_max[shift_col]} en los demás\")\n",
        "    print(f\"   para reducir la brecha de {porcentaje:.0f}% en ingresos\")\n",
        "else:\n",
        "    print(\"   Mantener consistencia operativa, diferencias no significativas\")\n",
        "    print(\"   Enfocar en eficiencia por unidad más que volumen total\")\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)"
      ]
    },
