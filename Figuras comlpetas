    {
      "cell_type": "code",
      "source": [
        "# ============================================================\n",
        "# FIGURA 8: Defect_Rate por línea y turno (Boxplots 2 subgrupos)\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"GENERANDO FIGURA 8\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Crear la figura\n",
        "fig, ax = plt.subplots(figsize=(12, 6))\n",
        "\n",
        "# Obtener líneas de producción únicas\n",
        "lineas = sorted(df[production_line_col].unique())\n",
        "turnos = ['Morning', 'Evening', 'Night']\n",
        "\n",
        "# Configurar posición de las cajas\n",
        "ancho_caja = 0.25\n",
        "posiciones_base = range(len(lineas))\n",
        "\n",
        "# Colores para turnos\n",
        "colores_turnos = [colors_shift[turno] for turno in turnos]\n",
        "\n",
        "# Lista para almacenar handles de leyenda\n",
        "handles = []\n",
        "labels = []\n",
        "\n",
        "# Crear boxplots para cada combinación línea-turno\n",
        "for i, linea in enumerate(lineas):\n",
        "    for j, turno in enumerate(turnos):\n",
        "        # Datos para esta combinación\n",
        "        datos = df[(df[production_line_col] == linea) & (df[shift_col] == turno)][defect_rate_col].dropna()\n",
        "\n",
        "        if len(datos) > 0:  # Solo graficar si hay datos\n",
        "            # Calcular posición\n",
        "            posicion = posiciones_base[i] + (j - 1) * ancho_caja\n",
        "\n",
        "            # Crear boxplot\n",
        "            bp = ax.boxplot(\n",
        "                datos,\n",
        "                positions=[posicion],\n",
        "                widths=ancho_caja * 0.8,\n",
        "                patch_artist=True,\n",
        "                notch=True,\n",
        "                showmeans=True,\n",
        "                meanline=True,\n",
        "                meanprops={'color': 'blue', 'linestyle': '--', 'linewidth': 1},\n",
        "                medianprops={'color': 'black', 'linewidth': 2},\n",
        "                whiskerprops={'color': '#222', 'linewidth': 0.8},\n",
        "                capprops={'color': '#222', 'linewidth': 0.8},\n",
        "                flierprops={'marker': 'o', 'markerfacecolor': 'gray', 'markersize': 3, 'alpha': 0.5}\n",
        "            )\n",
        "\n",
        "            # Colorear la caja\n",
        "            bp['boxes'][0].set_facecolor(colores_turnos[j])\n",
        "            bp['boxes'][0].set_alpha(0.7)\n",
        "            bp['boxes'][0].set_edgecolor('black')\n",
        "            bp['boxes'][0].set_linewidth(1)\n",
        "\n",
        "            # Guardar para leyenda (solo primera línea)\n",
        "            if i == 0:\n",
        "                handles.append(bp['boxes'][0])\n",
        "                labels.append(turno)\n",
        "\n",
        "# Configurar eje X\n",
        "ax.set_xticks(posiciones_base)\n",
        "ax.set_xticklabels(lineas, fontsize=11)\n",
        "ax.set_xlabel('Production Line', fontsize=11)\n",
        "\n",
        "# Configurar eje Y\n",
        "ax.set_ylabel('Defect Rate', fontsize=11)\n",
        "ax.yaxis.set_major_formatter(plt.FuncFormatter(lambda y, _: f'{y:.0f}%'))\n",
        "\n",
        "# Configurar cuadrícula\n",
        "ax.grid(True, linestyle='--', linewidth=0.7, alpha=0.6, axis='y')\n",
        "\n",
        "# Configurar bordes\n",
        "for spine in ax.spines.values():\n",
        "    spine.set_color('#222')\n",
        "    spine.set_linewidth(1.2)\n",
        "\n",
        "# Título\n",
        "ax.set_title('Fig. 8: Defect_Rate por línea y turno (Boxplots)', fontsize=13, fontweight='bold', pad=15)\n",
        "\n",
        "# Leyenda\n",
        "ax.legend(handles, labels, title='Shift', loc='upper right', frameon=True)\n",
        "\n",
        "# Ajustar límites del eje Y para dar espacio\n",
        "y_min = df[defect_rate_col].min() - 2\n",
        "y_max = df[defect_rate_col].max() + 2\n",
        "ax.set_ylim(y_min, y_max)\n",
        "\n",
        "# Ajustar layout\n",
        "plt.tight_layout()\n",
        "\n",
        "# Mostrar figura\n",
        "plt.show()\n",
        "\n",
        "# ============================================================\n",
        "# INTERPRETACIÓN DE LA FIGURA 8\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"INTERPRETACIÓN FIGURA 8\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# ============================================================\n",
        "# 1. ESTADÍSTICAS POR COMBINACIÓN LÍNEA-TURNO\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n1. ESTADÍSTICAS POR LÍNEA Y TURNO:\")\n",
        "print(\"-\" * 60)\n",
        "\n",
        "for linea in lineas:\n",
        "    print(f\"\\n{linea}:\")\n",
        "    for turno in turnos:\n",
        "        datos = df[(df[production_line_col] == linea) & (df[shift_col] == turno)][defect_rate_col].dropna()\n",
        "\n",
        "        if len(datos) > 0:\n",
        "            Q1 = datos.quantile(0.25)\n",
        "            Q3 = datos.quantile(0.75)\n",
        "            IQR = Q3 - Q1\n",
        "\n",
        "            print(f\"  {turno}:\")\n",
        "            print(f\"    Media: {datos.mean():.2f}%\")\n",
        "            print(f\"    Mediana: {datos.median():.2f}%\")\n",
        "            print(f\"    Std: {datos.std():.2f}%\")\n",
        "            print(f\"    Min: {datos.min():.2f}%\")\n",
        "            print(f\"    Max: {datos.max():.2f}%\")\n",
        "            print(f\"    IQR: {IQR:.2f}%\")\n",
        "            print(f\"    n={len(datos)}\")\n",
        "\n",
        "# ============================================================\n",
        "# 2. ANÁLISIS POR LÍNEA\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"2. ANÁLISIS POR LÍNEA DE PRODUCCIÓN\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "for linea in lineas:\n",
        "    print(f\"\\n{linea}:\")\n",
        "\n",
        "    # Datos por turno para esta línea\n",
        "    datos_turnos = {}\n",
        "    for turno in turnos:\n",
        "        datos = df[(df[production_line_col] == linea) & (df[shift_col] == turno)][defect_rate_col].dropna()\n",
        "        if len(datos) > 0:\n",
        "            datos_turnos[turno] = datos\n",
        "\n",
        "    if len(datos_turnos) > 0:\n",
        "        # Encontrar mejor y peor turno\n",
        "        medias = {turno: datos.mean() for turno, datos in datos_turnos.items()}\n",
        "        mejor_turno = min(medias, key=medias.get)\n",
        "        peor_turno = max(medias, key=medias.get)\n",
        "\n",
        "        print(f\"  Mejor turno (menor defectos): {mejor_turno} ({medias[mejor_turno]:.2f}%)\")\n",
        "        print(f\"  Peor turno (mayor defectos): {peor_turno} ({medias[peor_turno]:.2f}%)\")\n",
        "\n",
        "        # Diferencia entre turnos\n",
        "        if len(medias) > 1:\n",
        "            rango = max(medias.values()) - min(medias.values())\n",
        "            print(f\"  Rango entre turnos: {rango:.2f}%\")\n",
        "\n",
        "        # Variabilidad dentro de la línea\n",
        "        stds = {turno: datos.std() for turno, datos in datos_turnos.items()}\n",
        "        turno_mas_variable = max(stds, key=stds.get)\n",
        "        print(f\"  Turno más variable: {turno_mas_variable} (σ={stds[turno_mas_variable]:.2f}%)\")\n",
        "\n",
        "# ============================================================\n",
        "# 3. ANÁLISIS POR TURNO\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"3. ANÁLISIS POR TURNO\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "for turno in turnos:\n",
        "    print(f\"\\n{turno}:\")\n",
        "\n",
        "    # Datos por línea para este turno\n",
        "    datos_lineas = {}\n",
        "    for linea in lineas:\n",
        "        datos = df[(df[production_line_col] == linea) & (df[shift_col] == turno)][defect_rate_col].dropna()\n",
        "        if len(datos) > 0:\n",
        "            datos_lineas[linea] = datos\n",
        "\n",
        "    if len(datos_lineas) > 0:\n",
        "        # Encontrar mejor y peor línea\n",
        "        medias = {linea: datos.mean() for linea, datos in datos_lineas.items()}\n",
        "        mejor_linea = min(medias, key=medias.get)\n",
        "        peor_linea = max(medias, key=medias.get)\n",
        "\n",
        "        print(f\"  Mejor línea (menor defectos): {mejor_linea} ({medias[mejor_linea]:.2f}%)\")\n",
        "        print(f\"  Peor línea (mayor defectos): {peor_linea} ({medias[peor_linea]:.2f}%)\")\n",
        "\n",
        "        # Diferencia entre líneas\n",
        "        if len(medias) > 1:\n",
        "            rango = max(medias.values()) - min(medias.values())\n",
        "            print(f\"  Rango entre líneas: {rango:.2f}%\")\n",
        "\n",
        "# ============================================================\n",
        "# 4. INTERACCIÓN LÍNEA × TURNO\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"4. INTERACCIÓN LÍNEA × TURNO\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Crear tabla de medias\n",
        "tabla_medias = pd.DataFrame(index=lineas, columns=turnos)\n",
        "\n",
        "for linea in lineas:\n",
        "    for turno in turnos:\n",
        "        datos = df[(df[production_line_col] == linea) & (df[shift_col] == turno)][defect_rate_col].dropna()\n",
        "        if len(datos) > 0:\n",
        "            tabla_medias.loc[linea, turno] = datos.mean()\n",
        "\n",
        "print(\"\\nTabla de medias (% defectos):\")\n",
        "print(tabla_medias.round(2))\n",
        "\n",
        "# Detectar interacciones\n",
        "print(\"\\nPatrones de interacción:\")\n",
        "\n",
        "# Verificar si el mejor turno es consistente entre líneas\n",
        "mejores_turnos = {}\n",
        "for linea in lineas:\n",
        "    mejor = None\n",
        "    mejor_valor = float('inf')\n",
        "    for turno in turnos:\n",
        "        valor = tabla_medias.loc[linea, turno]\n",
        "        if pd.notna(valor) and valor < mejor_valor:\n",
        "            mejor_valor = valor\n",
        "            mejor = turno\n",
        "    mejores_turnos[linea] = mejor\n",
        "\n",
        "if len(set(mejores_turnos.values())) == 1:\n",
        "    turno_consistente = list(mejores_turnos.values())[0]\n",
        "    print(f\"  → El turno {turno_consistente} es el mejor en TODAS las líneas\")\n",
        "    print(\"     No hay interacción: el efecto del turno es consistente\")\n",
        "else:\n",
        "    print(\"  → El mejor turno VARÍA según la línea\")\n",
        "    for linea, turno in mejores_turnos.items():\n",
        "        print(f\"     {linea}: mejor turno = {turno}\")\n",
        "    print(\"     HAY INTERACCIÓN: el efecto del turno depende de la línea\")\n",
        "\n",
        "# Verificar si la mejor línea es consistente entre turnos\n",
        "mejores_lineas = {}\n",
        "for turno in turnos:\n",
        "    mejor = None\n",
        "    mejor_valor = float('inf')\n",
        "    for linea in lineas:\n",
        "        valor = tabla_medias.loc[linea, turno]\n",
        "        if pd.notna(valor) and valor < mejor_valor:\n",
        "            mejor_valor = valor\n",
        "            mejor = linea\n",
        "    mejores_lineas[turno] = mejor\n",
        "\n",
        "if len(set(mejores_lineas.values())) == 1:\n",
        "    linea_consistente = list(mejores_lineas.values())[0]\n",
        "    print(f\"\\n  → La línea {linea_consistente} es la mejor en TODOS los turnos\")\n",
        "else:\n",
        "    print(f\"\\n  → La mejor línea VARÍA según el turno\")\n",
        "\n",
        "# ============================================================\n",
        "# 5. PRUEBAS ESTADÍSTICAS\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"5. PRUEBAS ESTADÍSTICAS\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "from scipy import stats\n",
        "\n",
        "# ANOVA de dos vías (si hay suficientes datos)\n",
        "print(\"\\nANOVA de dos vías (Línea × Turno):\")\n",
        "\n",
        "try:\n",
        "    import statsmodels.api as sm\n",
        "    from statsmodels.formula.api import ols\n",
        "\n",
        "    # Preparar datos para ANOVA\n",
        "    df_anova = df[[production_line_col, shift_col, defect_rate_col]].dropna()\n",
        "    df_anova.columns = ['Linea', 'Turno', 'Defect_Rate']\n",
        "\n",
        "    # Modelo ANOVA\n",
        "    modelo = ols('Defect_Rate ~ C(Linea) + C(Turno) + C(Linea):C(Turno)', data=df_anova).fit()\n",
        "    tabla_anova = sm.stats.anova_lm(modelo, typ=2)\n",
        "\n",
        "    print(tabla_anova)\n",
        "\n",
        "    # Interpretar resultados\n",
        "    p_linea = tabla_anova.loc['C(Linea)', 'PR(>F)']\n",
        "    p_turno = tabla_anova.loc['C(Turno)', 'PR(>F)']\n",
        "    p_interaccion = tabla_anova.loc['C(Linea):C(Turno)', 'PR(>F)']\n",
        "\n",
        "    print(\"\\nInterpretación:\")\n",
        "    if p_linea < 0.05:\n",
        "        print(f\"  → Efecto SIGNIFICATIVO de Línea (p={p_linea:.4f})\")\n",
        "    else:\n",
        "        print(f\"  → Efecto NO significativo de Línea (p={p_linea:.4f})\")\n",
        "\n",
        "    if p_turno < 0.05:\n",
        "        print(f\"  → Efecto SIGNIFICATIVO de Turno (p={p_turno:.4f})\")\n",
        "    else:\n",
        "        print(f\"  → Efecto NO significativo de Turno (p={p_turno:.4f})\")\n",
        "\n",
        "    if p_interaccion < 0.05:\n",
        "        print(f\"  → INTERACCIÓN SIGNIFICATIVA (p={p_interaccion:.4f})\")\n",
        "        print(\"     El efecto del turno depende de la línea (y viceversa)\")\n",
        "    else:\n",
        "        print(f\"  → Interacción NO significativa (p={p_interaccion:.4f})\")\n",
        "        print(\"     Los efectos son aditivos e independientes\")\n",
        "\n",
        "except Exception as e:\n",
        "    print(f\"  No se pudo realizar ANOVA: {e}\")\n",
        "\n",
        "    # Pruebas t por pares como alternativa\n",
        "    print(\"\\nPruebas t por pares (comparaciones):\")\n",
        "    for i, linea in enumerate(lineas):\n",
        "        for j, turno in enumerate(turnos):\n",
        "            for k in range(j+1, len(turnos)):\n",
        "                turno2 = turnos[k]\n",
        "                datos1 = df[(df[production_line_col] == linea) & (df[shift_col] == turno)][defect_rate_col].dropna()\n",
        "                datos2 = df[(df[production_line_col] == linea) & (df[shift_col] == turno2)][defect_rate_col].dropna()\n",
        "\n",
        "                if len(datos1) > 3 and len(datos2) > 3:\n",
        "                    t_stat, p_value = stats.ttest_ind(datos1, datos2)\n",
        "                    if p_value < 0.05:\n",
        "                        print(f\"  {linea} - {turno} vs {turno2}: DIFERENCIA (p={p_value:.4f})\")\n",
        "\n",
        "# ============================================================\n",
        "# 6. INTERPRETACIÓN FINAL\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"6. INTERPRETACIÓN FINAL\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Calcular estadísticas globales\n",
        "mejor_combinacion = None\n",
        "mejor_valor = float('inf')\n",
        "peor_combinacion = None\n",
        "peor_valor = float('-inf')\n",
        "\n",
        "for linea in lineas:\n",
        "    for turno in turnos:\n",
        "        datos = df[(df[production_line_col] == linea) & (df[shift_col] == turno)][defect_rate_col].dropna()\n",
        "        if len(datos) > 0:\n",
        "            media = datos.mean()\n",
        "            if media < mejor_valor:\n",
        "                mejor_valor = media\n",
        "                mejor_combinacion = (linea, turno)\n",
        "            if media > peor_valor:\n",
        "                peor_valor = media\n",
        "                peor_combinacion = (linea, turno)\n",
        "\n",
        "print(f\"\\nMejor combinación (menos defectos):\")\n",
        "print(f\"  {mejor_combinacion[0]} - {mejor_combinacion[1]}: {mejor_valor:.2f}%\")\n",
        "\n",
        "print(f\"\\nPeor combinación (más defectos):\")\n",
        "print(f\"  {peor_combinacion[0]} - {peor_combinacion[1]}: {peor_valor:.2f}%\")\n",
        "\n",
        "# Recomendaciones\n",
        "print(\"\\nRECOMENDACIONES:\")\n",
        "\n",
        "# Basado en mejor combinación\n",
        "print(f\"\\n1. Benchmarking:\")\n",
        "print(f\"   La combinación {mejor_combinacion[0]}-{mejor_combinacion[1]} tiene la mejor calidad\")\n",
        "print(f\"   ({mejor_valor:.2f}% defectos). Analizar sus prácticas.\")\n",
        "\n",
        "# Basado en peor combinación\n",
        "print(f\"\\n2. Atención prioritaria:\")\n",
        "print(f\"   {peor_combinacion[0]}-{peor_combinacion[1]} requiere intervención urgente\")\n",
        "print(f\"   ({peor_valor:.2f}% defectos, {peor_valor/mejor_valor:.1f}x la mejor combinación)\")\n",
        "\n",
        "# Patrones por línea\n",
        "print(\"\\n3. Por línea de producción:\")\n",
        "for linea in lineas:\n",
        "    datos_linea = df[df[production_line_col] == linea][defect_rate_col]\n",
        "    media_linea = datos_linea.mean()\n",
        "\n",
        "    if media_linea < df[defect_rate_col].mean() * 0.9:\n",
        "        print(f\"   {linea}: Calidad superior al promedio\")\n",
        "    elif media_linea > df[defect_rate_col].mean() * 1.1:\n",
        "        print(f\"   {linea}: Calidad inferior al promedio - REQUIERE MEJORA\")\n",
        "    else:\n",
        "        print(f\"   {linea}: Calidad dentro del promedio\")\n",
        "\n",
        "# Patrones por turno\n",
        "print(\"\\n4. Por turno:\")\n",
        "for turno in turnos:\n",
        "    datos_turno = df[df[shift_col] == turno][defect_rate_col]\n",
        "    media_turno = datos_turno.mean()\n",
        "\n",
        "    if media_turno < df[defect_rate_col].mean() * 0.95:\n",
        "        print(f\"   {turno}: Mejor calidad que otros turnos\")\n",
        "    elif media_turno > df[defect_rate_col].mean() * 1.05:\n",
        "        print(f\"   {turno}: Peor calidad que otros turnos\")\n",
        "    else:\n",
        "        print(f\"   {turno}: Calidad similar al promedio\")\n",
        "\n",
        "# Recomendación final\n",
        "print(\"\\n5. ACCIONES SUGERIDAS:\")\n",
        "\n",
        "if peor_valor / mejor_valor > 2:\n",
        "    print(\"   • Reducir brecha de calidad: estandarizar procesos\")\n",
        "\n",
        "if len(set(mejores_turnos.values())) > 1:\n",
        "    print(\"   • Personalizar estrategias por línea (hay interacción)\")\n",
        "else:\n",
        "    print(\"   • Aplicar mismas prácticas en todas las líneas\")\n",
        "\n",
        "if max([df[df[shift_col] == turno][defect_rate_col].std() for turno in turnos]) > 5:\n",
        "    print(\"   • Reducir variabilidad dentro de cada turno\")\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)"
