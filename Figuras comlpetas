        "# ============================================================\n",
        "# FIGURA 5: Productividad promedio por línea (±EE)\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"GENERANDO FIGURA 5\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Crear la figura\n",
        "fig, ax = plt.subplots(figsize=(10, 6))\n",
        "\n",
        "# Calcular estadísticas por línea de producción\n",
        "stats_lineas = df.groupby(production_line_col)[units_col].agg(['mean', 'std', 'count']).reset_index()\n",
        "stats_lineas.columns = [production_line_col, 'media', 'std', 'count']\n",
        "\n",
        "# Calcular error estándar (EE = std / sqrt(n))\n",
        "stats_lineas['ee'] = stats_lineas['std'] / np.sqrt(stats_lineas['count'])\n",
        "\n",
        "# Ordenar por media descendente\n",
        "stats_lineas = stats_lineas.sort_values('media', ascending=False)\n",
        "\n",
        "# Colores para las barras (usando los colores de línea)\n",
        "bar_colors = [colors_lines[line] for line in stats_lineas[production_line_col]]\n",
        "\n",
        "# Crear barras\n",
        "bars = ax.bar(\n",
        "    stats_lineas[production_line_col],\n",
        "    stats_lineas['media'],\n",
        "    yerr=stats_lineas['ee'],\n",
        "    capsize=5,\n",
        "    color=bar_colors,\n",
        "    edgecolor='black',\n",
        "    linewidth=1,\n",
        "    alpha=0.8,\n",
        "    error_kw={'elinewidth': 1.5, 'ecolor': 'black', 'capthick': 1.5}\n",
        ")\n",
        "\n",
        "# Añadir etiquetas numéricas encima de cada barra\n",
        "for i, (idx, row) in enumerate(stats_lineas.iterrows()):\n",
        "    ax.text(\n",
        "        i,\n",
        "        row['media'] + row['ee'] + 5,  # Posición encima de la barra de error\n",
        "        f'{row[\"media\"]:.0f}',\n",
        "        ha='center',\n",
        "        va='bottom',\n",
        "        fontsize=10,\n",
        "        fontweight='bold'\n",
        "    )\n",
        "\n",
        "# Configurar ejes\n",
        "ax.set_xlabel('Production Line', fontsize=11)\n",
        "ax.set_ylabel('Units Produced (promedio)', fontsize=11)\n",
        "\n",
        "# Configurar ticks\n",
        "ax.yaxis.set_major_formatter(plt.FuncFormatter(lambda y, _: f'{int(y):,}'))\n",
        "\n",
        "# Configurar cuadrícula (solo eje Y)\n",
        "ax.grid(True, linestyle='--', linewidth=0.7, alpha=0.6, axis='y')\n",
        "\n",
        "# Configurar bordes\n",
        "for spine in ax.spines.values():\n",
        "    spine.set_color('#222')\n",
        "    spine.set_linewidth(1.2)\n",
        "\n",
        "# Título\n",
        "ax.set_title('Fig. 5: Productividad promedio por línea (±EE)', fontsize=13, fontweight='bold', pad=15)\n",
        "\n",
        "# Leyenda para error\n",
        "from matplotlib.patches import Patch\n",
        "from matplotlib.lines import Line2D\n",
        "legend_elements = [\n",
        "    Patch(facecolor='gray', alpha=0.5, label='Promedio'),\n",
        "    Line2D([0], [0], color='black', linewidth=1.5, label='± Error Estándar')\n",
        "]\n",
        "ax.legend(handles=legend_elements, loc='upper right', frameon=True)\n",
        "\n",
        "# Ajustar layout\n",
        "plt.tight_layout()\n",
        "\n",
        "# Mostrar figura\n",
        "plt.show()\n",
        "\n",
        "# ============================================================\n",
        "# INTERPRETACIÓN DE LA FIGURA 5\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"INTERPRETACIÓN FIGURA 5\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Mostrar estadísticas por línea\n",
        "print(\"\\nEstadísticas de productividad por línea:\")\n",
        "print(\"-\" * 50)\n",
        "\n",
        "for idx, row in stats_lineas.iterrows():\n",
        "    print(f\"\\n{row[production_line_col]}:\")\n",
        "    print(f\"  Producción promedio: {row['media']:.0f} unidades\")\n",
        "    print(f\"  Desviación estándar: ±{row['std']:.0f} unidades\")\n",
        "    print(f\"  Error estándar: ±{row['ee']:.1f} unidades\")\n",
        "    print(f\"  Número de lotes: {int(row['count'])}\")\n",
        "    print(f\"  Rango (media ± EE): {row['media'] - row['ee']:.0f} - {row['media'] + row['ee']:.0f} unidades\")\n",
        "\n",
        "# Ordenar líneas por productividad\n",
        "lineas_ordenadas = stats_lineas[production_line_col].tolist()\n",
        "print(f\"\\nOrden de productividad (de mayor a menor):\")\n",
        "for i, linea in enumerate(lineas_ordenadas, 1):\n",
        "    media = stats_lineas[stats_lineas[production_line_col] == linea]['media'].values[0]\n",
        "    print(f\"  {i}. {linea}: {media:.0f} unidades\")\n",
        "\n",
        "# Comparar línea líder vs última\n",
        "linea_top = lineas_ordenadas[0]\n",
        "linea_bottom = lineas_ordenadas[-1]\n",
        "media_top = stats_lineas[stats_lineas[production_line_col] == linea_top]['media'].values[0]\n",
        "media_bottom = stats_lineas[stats_lineas[production_line_col] == linea_bottom]['media'].values[0]\n",
        "diferencia = media_top - media_bottom\n",
        "porcentaje_mejora = (diferencia / media_bottom) * 100\n",
        "\n",
        "print(f\"\\nComparación líder vs último:\")\n",
        "print(f\"  {linea_top} produce {diferencia:.0f} unidades más que {linea_bottom}\")\n",
        "print(f\"  Esto representa un {porcentaje_mejora:.1f}% más de productividad\")\n",
        "\n",
        "# Análisis de diferencias significativas\n",
        "print(\"\\nAnálisis de diferencias estadísticas:\")\n",
        "\n",
        "for i in range(len(lineas_ordenadas)):\n",
        "    for j in range(i+1, len(lineas_ordenadas)):\n",
        "        linea1 = lineas_ordenadas[i]\n",
        "        linea2 = lineas_ordenadas[j]\n",
        "\n",
        "        data1 = df[df[production_line_col] == linea1][units_col]\n",
        "        data2 = df[df[production_line_col] == linea2][units_col]\n",
        "\n",
        "        from scipy import stats\n",
        "        t_stat, p_value = stats.ttest_ind(data1, data2)\n",
        "\n",
        "        if p_value < 0.05:\n",
        "            print(f\"  {linea1} vs {linea2}: DIFERENCIA SIGNIFICATIVA (p={p_value:.4f})\")\n",
        "        else:\n",
        "            print(f\"  {linea1} vs {linea2}: Diferencia no significativa (p={p_value:.4f})\")\n",
        "\n",
        "# Análisis de variabilidad\n",
        "print(f\"\\nAnálisis de variabilidad:\")\n",
        "linea_mas_variable = stats_lineas.loc[stats_lineas['std'].idxmax()]\n",
        "linea_mas_consistente = stats_lineas.loc[stats_lineas['std'].idxmin()]\n",
        "\n",
        "print(f\"  Línea más variable: {linea_mas_variable[production_line_col]} (std={linea_mas_variable['std']:.0f})\")\n",
        "print(f\"  Línea más consistente: {linea_mas_consistente[production_line_col]} (std={linea_mas_consistente['std']:.0f})\")\n",
        "\n",
        "# Interpretación de resultados\n",
        "print(\"\\n\" + \"=\"*50)\n",
        "print(\"INTERPRETACIÓN DE RESULTADOS\")\n",
        "print(\"=\"*50)\n",
        "\n",
        "# Encontrar la línea con mayor productividad\n",
        "linea_max = stats_lineas.loc[stats_lineas['media'].idxmax()]\n",
        "print(f\"\\n1. Línea más productiva:\")\n",
        "print(f\"   {linea_max[production_line_col]} lidera con {linea_max['media']:.0f} unidades promedio\")\n",
        "print(f\"   Error estándar: ±{linea_max['ee']:.1f} unidades\")\n",
        "\n",
        "# Determinar si hay diferencias claras\n",
        "diferencias_claras = False\n",
        "for i in range(len(lineas_ordenadas)):\n",
        "    for j in range(i+1, len(lineas_ordenadas)):\n",
        "        linea1 = lineas_ordenadas[i]\n",
        "        linea2 = lineas_ordenadas[j]\n",
        "        media1 = stats_lineas[stats_lineas[production_line_col] == linea1]['media'].values[0]\n",
        "        media2 = stats_lineas[stats_lineas[production_line_col] == linea2]['media'].values[0]\n",
        "        ee1 = stats_lineas[stats_lineas[production_line_col] == linea1]['ee'].values[0]\n",
        "        ee2 = stats_lineas[stats_lineas[production_line_col] == linea2]['ee'].values[0]\n",
        "\n",
        "        # Si los intervalos de confianza no se superponen\n",
        "        if (media1 - ee1) > (media2 + ee2) or (media2 - ee2) > (media1 + ee1):\n",
        "            diferencias_claras = True\n",
        "\n",
        "print(f\"\\n2. Diferencias entre líneas:\")\n",
        "if diferencias_claras:\n",
        "    print(\"   Hay diferencias estadísticamente claras entre líneas\")\n",
        "    print(\"   Los intervalos de error no se superponen en algunos casos\")\n",
        "else:\n",
        "    print(\"   Las diferencias no son estadísticamente concluyentes\")\n",
        "    print(\"   Los intervalos de error se superponen considerablemente\")\n",
        "\n",
        "# Recomendaciones\n",
        "print(f\"\\n3. Recomendaciones:\")\n",
        "\n",
        "# Recomendación para línea líder\n",
        "if linea_max[production_line_col] == 'Line_A':\n",
        "    print(f\"   - {linea_max[production_line_col]} es líder, analizar sus prácticas para replicar\")\n",
        "elif linea_max[production_line_col] == 'Line_B':\n",
        "    print(f\"   - {linea_max[production_line_col]} muestra mejor productividad, documentar métodos\")\n",
        "else:\n",
        "    print(f\"   - {linea_max[production_line_col]} es la más productiva, investigar factores clave\")\n",
        "\n",
        "# Recomendación para línea con mayor variabilidad\n",
        "if linea_mas_variable['std'] > 50:\n",
        "    print(f\"   - {linea_mas_variable[production_line_col]} tiene alta variabilidad, estandarizar procesos\")\n",
        "\n",
        "# Recomendación general\n",
        "if porcentaje_mejora > 20:\n",
        "    print(f\"   - Brecha del {porcentaje_mejora:.0f}% entre líneas, implementar mejoras en la menos productiva\")\n",
        "else:\n",
        "    print(f\"   - Brecha pequeña ({porcentaje_mejora:.0f}%), mantener consistencia operativa\")\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)"
