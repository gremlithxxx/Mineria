      "cell_type": "markdown",
      "source": [
        "#Figura 11"
      ],
      "metadata": {
        "id": "QFyMxyNnlBNJ"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# ============================================================\n",
        "# FIGURA 11: Tendencias Units Produced por Línea (±EE) a través de Batch\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"GENERANDO FIGURA 11\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Crear la figura\n",
        "fig, ax = plt.subplots(figsize=(12, 6))\n",
        "\n",
        "# Obtener batches únicos ordenados\n",
        "batches = sorted(df[batch_col].unique())\n",
        "\n",
        "# Para cada línea de producción\n",
        "for linea in df[production_line_col].unique():\n",
        "\n",
        "    # Calcular media y error estándar por batch para esta línea\n",
        "    stats_batch = []\n",
        "    for batch in batches:\n",
        "        datos_batch = df[(df[production_line_col] == linea) & (df[batch_col] == batch)][units_col]\n",
        "        if len(datos_batch) > 0:\n",
        "            media = datos_batch.mean()\n",
        "            std = datos_batch.std()\n",
        "            ee = std / np.sqrt(len(datos_batch))\n",
        "            stats_batch.append({\n",
        "                'batch': batch,\n",
        "                'media': media,\n",
        "                'ee': ee,\n",
        "                'n': len(datos_batch)\n",
        "            })\n",
        "\n",
        "    # Convertir a DataFrame para facilitar\n",
        "    if stats_batch:\n",
        "        df_linea = pd.DataFrame(stats_batch)\n",
        "\n",
        "        # Graficar línea de media\n",
        "        ax.plot(df_linea['batch'], df_linea['media'],\n",
        "                color=colors_lines[linea],\n",
        "                marker=markers_lines[linea],\n",
        "                linewidth=2,\n",
        "                markersize=6,\n",
        "                label=linea)\n",
        "\n",
        "        # Añadir banda de error estándar\n",
        "        ax.fill_between(df_linea['batch'],\n",
        "                        df_linea['media'] - df_linea['ee'],\n",
        "                        df_linea['media'] + df_linea['ee'],\n",
        "                        color=colors_lines[linea],\n",
        "                        alpha=0.2)\n",
        "\n",
        "        # Etiquetar el último punto\n",
        "        ultimo = df_linea.iloc[-1]\n",
        "        ax.annotate(f'{linea}',\n",
        "                   xy=(ultimo['batch'], ultimo['media']),\n",
        "                   xytext=(5, 0),\n",
        "                   textcoords='offset points',\n",
        "                   fontsize=10,\n",
        "                   fontweight='bold',\n",
        "                   color=colors_lines[linea])\n",
        "\n",
        "# Configurar ejes\n",
        "ax.set_xlabel('Batch', fontsize=11)\n",
        "ax.set_ylabel('Units Produced (media)', fontsize=11)\n",
        "\n",
        "# Configurar ticks\n",
        "ax.set_xticks(batches)\n",
        "ax.yaxis.set_major_formatter(plt.FuncFormatter(lambda y, _: f'{int(y):,}'))\n",
        "\n",
        "# Configurar cuadrícula\n",
        "ax.grid(True, linestyle='--', linewidth=0.7, alpha=0.6)\n",
        "\n",
        "# Configurar bordes\n",
        "for spine in ax.spines.values():\n",
        "    spine.set_color('#222')\n",
        "    spine.set_linewidth(1.2)\n",
        "\n",
        "# Título\n",
        "ax.set_title('Fig. 11: Tendencias Units Produced por Línea (±EE)', fontsize=13, fontweight='bold', pad=15)\n",
        "\n",
        "# Leyenda (sin duplicar las que ya tienen etiqueta)\n",
        "ax.legend(title='Production Line', loc='best', frameon=True)\n",
        "\n",
        "# Ajustar layout\n",
        "plt.tight_layout()\n",
        "\n",
        "# Mostrar figura\n",
        "plt.show()\n",
        "\n",
        "# ============================================================\n",
        "# INTERPRETACIÓN DE LA FIGURA 11\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"INTERPRETACIÓN FIGURA 11\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# ============================================================\n",
        "# 1. ESTADÍSTICAS POR LÍNEA Y BATCH\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n1. ESTADÍSTICAS POR LÍNEA Y BATCH:\")\n",
        "print(\"-\" * 60)\n",
        "\n",
        "for linea in df[production_line_col].unique():\n",
        "    print(f\"\\n{linea}:\")\n",
        "\n",
        "    # Estadísticas por batch\n",
        "    for batch in batches:\n",
        "        datos = df[(df[production_line_col] == linea) & (df[batch_col] == batch)][units_col]\n",
        "        if len(datos) > 0:\n",
        "            media = datos.mean()\n",
        "            ee = datos.std() / np.sqrt(len(datos))\n",
        "            print(f\"  Batch {batch}: media = {media:.0f} ± {ee:.1f} (n={len(datos)})\")\n",
        "\n",
        "# ============================================================\n",
        "# 2. ANÁLISIS DE TENDENCIAS POR LÍNEA\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"2. ANÁLISIS DE TENDENCIAS POR LÍNEA\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "for linea in df[production_line_col].unique():\n",
        "    print(f\"\\n{linea}:\")\n",
        "\n",
        "    # Recolectar medias por batch\n",
        "    medias_batch = []\n",
        "    batches_linea = []\n",
        "    for batch in batches:\n",
        "        datos = df[(df[production_line_col] == linea) & (df[batch_col] == batch)][units_col]\n",
        "        if len(datos) > 0:\n",
        "            medias_batch.append(datos.mean())\n",
        "            batches_linea.append(batch)\n",
        "\n",
        "    if len(medias_batch) >= 2:\n",
        "        # Calcular tendencia (primer vs último batch)\n",
        "        primero = medias_batch[0]\n",
        "        ultimo = medias_batch[-1]\n",
        "        cambio = ultimo - primero\n",
        "        cambio_porcentual = (cambio / primero) * 100\n",
        "\n",
        "        print(f\"  Batch {batches_linea[0]}: {primero:.0f} unidades\")\n",
        "        print(f\"  Batch {batches_linea[-1]}: {ultimo:.0f} unidades\")\n",
        "        print(f\"  Cambio: {cambio:+.0f} unidades ({cambio_porcentual:+.1f}%)\")\n",
        "\n",
        "        # Determinar tendencia\n",
        "        if cambio_porcentual > 5:\n",
        "            print(f\"  → Tendencia POSITIVA (mejora de {cambio_porcentual:.1f}%)\")\n",
        "        elif cambio_porcentual < -5:\n",
        "            print(f\"  → Tendencia NEGATIVA (degradación de {abs(cambio_porcentual):.1f}%)\")\n",
        "        else:\n",
        "            print(f\"  → Tendencia ESTABLE (cambio < 5%)\")\n",
        "\n",
        "        # Calcular pendiente con regresión lineal\n",
        "        from scipy import stats\n",
        "        slope, intercept, r_value, p_value, std_err = stats.linregress(batches_linea, medias_batch)\n",
        "\n",
        "        print(f\"  Pendiente: {slope:.2f} unidades/batch\")\n",
        "        if p_value < 0.05:\n",
        "            print(f\"  Tendencia ESTADÍSTICAMENTE SIGNIFICATIVA (p={p_value:.4f})\")\n",
        "        else:\n",
        "            print(f\"  Tendencia NO significativa (p={p_value:.4f})\")\n",
        "\n",
        "# ============================================================\n",
        "# 3. COMPARACIÓN ENTRE LÍNEAS\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"3. COMPARACIÓN ENTRE LÍNEAS\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Comparar nivel de producción\n",
        "print(\"\\nNIVEL DE PRODUCCIÓN PROMEDIO:\")\n",
        "for linea in df[production_line_col].unique():\n",
        "    media_global = df[df[production_line_col] == linea][units_col].mean()\n",
        "    print(f\"  {linea}: {media_global:.0f} unidades\")\n",
        "\n",
        "# Línea con mayor producción\n",
        "linea_max = df.groupby(production_line_col)[units_col].mean().idxmax()\n",
        "media_max = df.groupby(production_line_col)[units_col].mean().max()\n",
        "print(f\"\\nLínea con mayor producción: {linea_max} ({media_max:.0f} unidades)\")\n",
        "\n",
        "# Línea con menor producción\n",
        "linea_min = df.groupby(production_line_col)[units_col].mean().idxmin()\n",
        "media_min = df.groupby(production_line_col)[units_col].mean().min()\n",
        "print(f\"Línea con menor producción: {linea_min} ({media_min:.0f} unidades)\")\n",
        "\n",
        "# Comparar tendencias\n",
        "print(\"\\nCOMPARACIÓN DE TENDENCIAS:\")\n",
        "lineas_con_tendencia = []\n",
        "for linea in df[production_line_col].unique():\n",
        "    medias_batch = []\n",
        "    batches_linea = []\n",
        "    for batch in batches:\n",
        "        datos = df[(df[production_line_col] == linea) & (df[batch_col] == batch)][units_col]\n",
        "        if len(datos) > 0:\n",
        "            medias_batch.append(datos.mean())\n",
        "            batches_linea.append(batch)\n",
        "\n",
        "    if len(medias_batch) >= 2:\n",
        "        slope, intercept, r_value, p_value, std_err = stats.linregress(batches_linea, medias_batch)\n",
        "        lineas_con_tendencia.append((linea, slope, p_value))\n",
        "\n",
        "        if slope > 0:\n",
        "            print(f\"  {linea}: pendiente positiva ({slope:.2f} uds/batch)\")\n",
        "        elif slope < 0:\n",
        "            print(f\"  {linea}: pendiente negativa ({slope:.2f} uds/batch)\")\n",
        "        else:\n",
        "            print(f\"  {linea}: pendiente plana\")\n",
        "\n",
        "# ============================================================\n",
        "# 4. ANÁLISIS DE VARIABILIDAD (ANCHO DE BANDA)\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"4. ANÁLISIS DE VARIABILIDAD (ERROR ESTÁNDAR)\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "print(\"\\nEl ancho de las bandas indica la incertidumbre/consistencia:\")\n",
        "\n",
        "for linea in df[production_line_col].unique():\n",
        "    ees = []\n",
        "    for batch in batches:\n",
        "        datos = df[(df[production_line_col] == linea) & (df[batch_col] == batch)][units_col]\n",
        "        if len(datos) > 1:\n",
        "            ee = datos.std() / np.sqrt(len(datos))\n",
        "            ees.append(ee)\n",
        "\n",
        "    if ees:\n",
        "        ee_promedio = np.mean(ees)\n",
        "        cv_promedio = (ee_promedio / df[df[production_line_col] == linea][units_col].mean()) * 100\n",
        "\n",
        "        print(f\"\\n{linea}:\")\n",
        "        print(f\"  Error estándar promedio: ±{ee_promedio:.1f} unidades\")\n",
        "        print(f\"  Coeficiente de variación del EE: {cv_promedio:.1f}%\")\n",
        "\n",
        "        if cv_promedio < 5:\n",
        "            print(f\"  → MUY CONSISTENTE (baja incertidumbre)\")\n",
        "        elif cv_promedio < 10:\n",
        "            print(f\"  → CONSISTENTE (incertidumbre moderada)\")\n",
        "        else:\n",
        "            print(f\"  → VARIABLE (alta incertidumbre)\")\n",
        "\n",
        "# ============================================================\n",
        "# 5. DETECCIÓN DE PUNTOS DE INFLEXIÓN\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"5. DETECCIÓN DE PUNTOS DE INFLEXIÓN\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "for linea in df[production_line_col].unique():\n",
        "    medias_batch = []\n",
        "    batches_linea = []\n",
        "    for batch in batches:\n",
        "        datos = df[(df[production_line_col] == linea) & (df[batch_col] == batch)][units_col]\n",
        "        if len(datos) > 0:\n",
        "            medias_batch.append(datos.mean())\n",
        "            batches_linea.append(batch)\n",
        "\n",
        "    if len(medias_batch) >= 3:\n",
        "        # Detectar cambios bruscos\n",
        "        cambios_bruscos = []\n",
        "        for i in range(1, len(medias_batch)):\n",
        "            cambio = medias_batch[i] - medias_batch[i-1]\n",
        "            cambio_porcentual = (cambio / medias_batch[i-1]) * 100\n",
        "            if abs(cambio_porcentual) > 15:\n",
        "                cambios_bruscos.append((batches_linea[i], cambio_porcentual))\n",
        "\n",
        "        if cambios_bruscos:\n",
        "            print(f\"\\n{linea} - Cambios bruscos detectados:\")\n",
        "            for batch, cambio in cambios_bruscos:\n",
        "                print(f\"  Batch {batch}: {cambio:+.1f}%\")\n",
        "        else:\n",
        "            print(f\"\\n{linea}: Sin cambios bruscos (>15%)\")\n",
        "\n",
        "# ============================================================\n",
        "# 6. INTERPRETACIÓN FINAL\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"6. INTERPRETACIÓN FINAL\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# 6.1 Tendencia global\n",
        "print(\"\\nTENDENCIA GLOBAL:\")\n",
        "\n",
        "tendencias_positivas = sum(1 for l, s, p in lineas_con_tendencia if s > 0 and p < 0.05)\n",
        "tendencias_negativas = sum(1 for l, s, p in lineas_con_tendencia if s < 0 and p < 0.05)\n",
        "\n",
        "if tendencias_positivas > tendencias_negativas:\n",
        "    print(\"  → Mejora general en productividad\")\n",
        "elif tendencias_negativas > tendencias_positivas:\n",
        "    print(\"  → Degradación general en productividad\")\n",
        "else:\n",
        "    print(\"  → Estabilidad general en productividad\")\n",
        "\n",
        "# 6.2 Línea destacada\n",
        "print(\"\\nLÍNEA DESTACADA:\")\n",
        "\n",
        "# Encontrar línea con mejor tendencia\n",
        "mejor_tendencia = None\n",
        "mejor_pendiente = -float('inf')\n",
        "for linea, slope, p in lineas_con_tendencia:\n",
        "    if slope > mejor_pendiente and p < 0.05:\n",
        "        mejor_pendiente = slope\n",
        "        mejor_tendencia = linea\n",
        "\n",
        "if mejor_tendencia:\n",
        "    print(f\"  {mejor_tendencia} muestra la mejora más significativa\")\n",
        "    print(f\"  Pendiente: {mejor_pendiente:.2f} unidades/batch\")\n",
        "\n",
        "# 6.3 Línea preocupante\n",
        "peor_tendencia = None\n",
        "peor_pendiente = float('inf')\n",
        "for linea, slope, p in lineas_con_tendencia:\n",
        "    if slope < peor_pendiente and p < 0.05:\n",
        "        peor_pendiente = slope\n",
        "        peor_tendencia = linea\n",
        "\n",
        "if peor_tendencia:\n",
        "    print(f\"\\n  {peor_tendencia} requiere atención (tendencia negativa)\")\n",
        "    print(f\"  Pérdida: {abs(peor_pendiente):.2f} unidades/batch\")\n",
        "\n",
        "# 6.4 Consistencia\n",
        "print(\"\\nCONSISTENCIA:\")\n",
        "\n",
        "linea_mas_consistente = None\n",
        "menor_cv = float('inf')\n",
        "for linea in df[production_line_col].unique():\n",
        "    cv = df[df[production_line_col] == linea][units_col].std() / df[df[production_line_col] == linea][units_col].mean() * 100\n",
        "    if cv < menor_cv:\n",
        "        menor_cv = cv\n",
        "        linea_mas_consistente = linea\n",
        "\n",
        "print(f\"  Línea más consistente: {linea_mas_consistente} (CV={menor_cv:.1f}%)\")\n",
        "\n",
        "# 6.5 Recomendaciones\n",
        "print(\"\\nRECOMENDACIONES:\")\n",
        "\n",
        "recomendaciones = []\n",
        "\n",
        "# Basado en tendencias\n",
        "if peor_tendencia:\n",
        "    recomendaciones.append(f\"• Investigar causas de caída en {peor_tendencia}\")\n",
        "    recomendaciones.append(f\"  (revisar mantenimiento, personal, insumos)\")\n",
        "\n",
        "if mejor_tendencia:\n",
        "    recomendaciones.append(f\"• Documentar prácticas de {mejor_tendencia} para compartir\")\n",
        "\n",
        "# Basado en consistencia\n",
        "if menor_cv > 15:\n",
        "    recomendaciones.append(\"• Alta variabilidad en todas las líneas - estandarizar procesos\")\n",
        "\n",
        "# Basado en cambios bruscos\n",
        "if cambios_bruscos:\n",
        "    recomendaciones.append(\"• Investigar causas de cambios bruscos entre batches\")\n",
        "    recomendaciones.append(\"  (posibles problemas de suministro o mantenimiento)\")\n",
        "\n",
        "# Basado en brechas\n",
        "brecha = media_max - media_min\n",
        "if brecha > 100:\n",
        "    recomendaciones.append(f\"• Brecha de {brecha:.0f} unidades entre líneas\")\n",
        "    recomendaciones.append(\"  Implementar mejoras en línea de menor producción\")\n",
        "\n",
        "# Mostrar recomendaciones\n",
        "if recomendaciones:\n",
        "    print(\"\\n\" + \"\\n\".join(recomendaciones[:5]))\n",
        "else:\n",
        "    print(\"\\nSin recomendaciones específicas - operación estable\")\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)"
