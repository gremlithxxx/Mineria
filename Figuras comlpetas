        "    asimetria = \"sesgo positivo (cola derecha más larga)\"\n",
        "    concentracion = \"valores bajos\"\n",
        "elif skewness < -0.5:\n",
        "    asimetria = \"sesgo negativo (cola izquierda más larga)\"\n",
        "    concentracion = \"valores altos\"\n",
        "else:\n",
        "    asimetria = \"aproximadamente simétrica\"\n",
        "    concentracion = \"centrada alrededor de la media\"\n",
        "\n",
        "print(f\"Forma: {asimetria}\")\n",
        "\n",
        "# Rangos de concentración\n",
        "print(\"\\nConcentración de datos:\")\n",
        "percentiles = [10, 25, 50, 75, 90]\n",
        "for p in percentiles:\n",
        "    valor = df[defect_rate_col].quantile(p/100)\n",
        "    print(f\"  Percentil {p}%: {valor:.2f}%\")\n",
        "\n",
        "# Interpretación de calidad\n",
        "print(\"\\nInterpretación de calidad:\")\n",
        "\n",
        "if mean_value < 5:\n",
        "    print(\"- La tasa de defectos promedio es BAJA (<5%) - Buena calidad\")\n",
        "elif mean_value < 10:\n",
        "    print(\"- La tasa de defectos promedio es MODERADA (5-10%) - Calidad aceptable\")\n",
        "else:\n",
        "    print(\"- La tasa de defectos promedio es ALTA (>10%) - Requiere mejora\")\n",
        "\n",
        "if df[defect_rate_col].max() > 3 * mean_value:\n",
        "    print(\"- Hay valores extremadamente altos que requieren investigación\")\n",
        "\n",
        "if abs(mean_value - median_value) > 2:\n",
        "    print(f\"- Diferencia media-mediana de {abs(mean_value - median_value):.1f}% indica asimetría\")\n",
        "    if mean_value > median_value:\n",
        "        print(\"  (media > mediana: algunos lotes con muchos defectos jalan el promedio)\")\n",
        "    else:\n",
        "        print(\"  (media < mediana: mayoría de lotes tienen defectos por encima del promedio)\")\n",
        "\n",
        "# Recomendaciones\n",
        "print(\"\\nRecomendaciones:\")\n",
        "if df[defect_rate_col].max() > 15:\n",
        "    print(\"- Investigar lotes con tasa de defectos superior a 15%\")\n",
        "if df[defect_rate_col].std() > 5:\n",
        "    print(\"- Alta variabilidad en calidad - estandarizar procesos\")\n",
        "if mean_value > 8:\n",
        "    print(\"- Implementar programa de mejora continua para reducir defectos\")\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)"
