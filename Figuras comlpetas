        "# ============================================================\n",
        "# FIGURA 4: Distribución de Downtime_Minutes (Histograma con KDE)\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"GENERANDO FIGURA 4\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Crear la figura con eje secundario\n",
        "fig, ax1 = plt.subplots(figsize=(12, 6))\n",
        "ax2 = ax1.twinx()  # Eje secundario para KDE\n",
        "\n",
        "# Calcular estadísticas\n",
        "p90 = df[downtime_col].quantile(0.90)\n",
        "p95 = df[downtime_col].quantile(0.95)\n",
        "moda_bin = None\n",
        "\n",
        "# Crear histograma con 15 bins\n",
        "n, bins, patches = ax1.hist(\n",
        "    df[downtime_col],\n",
        "    bins=15,\n",
        "    color='#f28e2b',\n",
        "    edgecolor='#1a1a1a',\n",
        "    linewidth=1.2,\n",
        "    alpha=0.7,\n",
        "    label='Frecuencia'\n",
        ")\n",
        "\n",
        "# Encontrar el bin con mayor frecuencia (moda)\n",
        "bin_centers = (bins[:-1] + bins[1:]) / 2\n",
        "max_freq_idx = np.argmax(n)\n",
        "moda_bin_center = bin_centers[max_freq_idx]\n",
        "\n",
        "# Línea de moda (bin)\n",
        "ax1.axvline(\n",
        "    moda_bin_center,\n",
        "    color='green',\n",
        "    linestyle='--',\n",
        "    linewidth=1.5,\n",
        "    label=f'Moda (bin): {moda_bin_center:.1f} min'\n",
        ")\n",
        "\n",
        "# Líneas de percentiles\n",
        "ax1.axvline(\n",
        "    p90,\n",
        "    color='blue',\n",
        "    linestyle=':',\n",
        "    linewidth=1.3,\n",
        "    label=f'P90: {p90:.1f} min'\n",
        ")\n",
        "ax1.axvline(\n",
        "    p95,\n",
        "    color='red',\n",
        "    linestyle='-.',\n",
        "    linewidth=1.3,\n",
        "    label=f'P95: {p95:.1f} min'\n",
        ")\n",
        "\n",
        "# Sombreado de cola derecha (a partir de P95)\n",
        "ax1.axvspan(\n",
        "    p95,\n",
        "    df[downtime_col].max(),\n",
        "    alpha=0.2,\n",
        "    color='red',\n",
        "    label='Cola derecha (P95+)'\n",
        ")\n",
        "\n",
        "# Curva KDE en eje secundario\n",
        "from scipy import stats\n",
        "kde = stats.gaussian_kde(df[downtime_col].dropna())\n",
        "x_range = np.linspace(df[downtime_col].min(), df[downtime_col].max(), 200)\n",
        "ax2.plot(\n",
        "    x_range,\n",
        "    kde(x_range),\n",
        "    color='#111',\n",
        "    linewidth=2,\n",
        "    label='Densidad (KDE)'\n",
        ")\n",
        "\n",
        "# Configurar eje primario\n",
        "ax1.set_xlabel('Downtime Minutes', fontsize=11)\n",
        "ax1.set_ylabel('Frecuencia', fontsize=11)\n",
        "ax1.yaxis.set_major_formatter(plt.FuncFormatter(lambda y, _: f'{int(y)}'))\n",
        "\n",
        "# Configurar eje secundario\n",
        "ax2.set_ylabel('Densidad', fontsize=11)\n",
        "ax2.set_ylim(0, ax2.get_ylim()[1] * 1.1)  # Ajustar límite superior\n",
        "\n",
        "# Configurar cuadrícula (solo en eje primario)\n",
        "ax1.grid(True, linestyle='--', linewidth=0.7, alpha=0.6)\n",
        "\n",
        "# Configurar bordes\n",
        "for spine in ax1.spines.values():\n",
        "    spine.set_color('#222')\n",
        "    spine.set_linewidth(1.2)\n",
        "\n",
        "# Título\n",
        "ax1.set_title('Fig. 4: Distribución de Downtime_Minutes', fontsize=13, fontweight='bold', pad=15)\n",
        "\n",
        "# Combinar leyendas de ambos ejes\n",
        "lines1, labels1 = ax1.get_legend_handles_labels()\n",
        "lines2, labels2 = ax2.get_legend_handles_labels()\n",
        "ax1.legend(lines1 + lines2, labels1 + labels2, loc='upper right', frameon=True)\n",
        "\n",
        "# Ajustar ticks\n",
        "plt.xticks(fontsize=10)\n",
        "ax1.yaxis.set_tick_params(labelsize=10)\n",
        "ax2.yaxis.set_tick_params(labelsize=10)\n",
        "\n",
        "# Ajustar layout\n",
        "plt.tight_layout()\n",
        "\n",
        "# Mostrar figura\n",
        "plt.show()\n",
        "\n",
        "# ============================================================\n",
        "# INTERPRETACIÓN DE LA FIGURA 4\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"INTERPRETACIÓN FIGURA 4\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Estadísticas de downtime\n",
        "print(\"\\nEstadísticas de Tiempo Muerto (minutos):\")\n",
        "print(f\"Media: {df[downtime_col].mean():.1f} min\")\n",
        "print(f\"Mediana: {df[downtime_col].median():.1f} min\")\n",
        "print(f\"Moda (bin): {moda_bin_center:.1f} min\")\n",
        "print(f\"Desviación estándar: {df[downtime_col].std():.1f} min\")\n",
        "print(f\"Mínimo: {df[downtime_col].min():.1f} min\")\n",
        "print(f\"Máximo: {df[downtime_col].max():.1f} min\")\n",
        "\n",
        "# Percentiles clave\n",
        "print(f\"\\nPercentiles:\")\n",
        "print(f\"  P90: {p90:.1f} min (90% de los datos están por debajo)\")\n",
        "print(f\"  P95: {p95:.1f} min (95% de los datos están por debajo)\")\n",
        "print(f\"  Cola derecha: {len(df[df[downtime_col] > p95])} registros ({len(df[df[downtime_col] > p95])/len(df)*100:.1f}%)\")\n",
        "\n",
        "# Análisis de asimetría\n",
        "skewness = df[downtime_col].skew()\n",
        "print(f\"\\nAsimetría: {skewness:.2f}\")\n",
        "\n",
        "if skewness > 1:\n",
        "    print(\"  Fuerte sesgo positivo - Cola derecha pronunciada\")\n",
        "    print(\"  Hay tiempos de paro excepcionalmente largos\")\n",
        "elif skewness > 0.5:\n",
        "    print(\"  Sesgo positivo moderado - Algunos paros largos\")\n",
        "else:\n",
        "    print(\"  Distribución aproximadamente simétrica\")\n",
        "\n",
        "# Análisis de multimodalidad (usando KDE)\n",
        "print(\"\\nAnálisis de modas:\")\n",
        "\n",
        "# Encontrar picos en KDE\n",
        "from scipy.signal import find_peaks\n",
        "kde_x = x_range\n",
        "kde_y = kde(x_range)\n",
        "peaks, _ = find_peaks(kde_y, height=0.001)\n",
        "\n",
        "if len(peaks) > 1:\n",
        "    print(f\"  Múltiples modas detectadas: {len(peaks)} picos\")\n",
        "    for i, peak_idx in enumerate(peaks):\n",
        "        print(f\"    Moda {i+1}: {kde_x[peak_idx]:.1f} min\")\n",
        "    print(\"  Esto sugiere diferentes regímenes de operación\")\n",
        "else:\n",
        "    print(\"  Distribución unimodal (un solo pico)\")\n",
        "\n",
        "# Interpretación operativa\n",
        "print(\"\\nInterpretación operativa:\")\n",
        "\n",
        "# Evaluar severidad de downtime\n",
        "mean_downtime = df[downtime_col].mean()\n",
        "if mean_downtime < 30:\n",
        "    print(\"- Tiempo muerto promedio BAJO (<30 min) - Operación eficiente\")\n",
        "elif mean_downtime < 60:\n",
        "    print(\"- Tiempo muerto promedio MODERADO (30-60 min) - Normal\")\n",
        "else:\n",
        "    print(\"- Tiempo muerto promedio ALTO (>60 min) - Requiere atención\")\n",
        "\n",
        "# Evaluar consistencia\n",
        "cv = df[downtime_col].std() / df[downtime_col].mean() * 100  # Coeficiente de variación\n",
        "if cv < 30:\n",
        "    print(f\"- Proceso CONSISTENTE (CV={cv:.0f}%) - Tiempos de paro predecibles\")\n",
        "elif cv < 60:\n",
        "    print(f\"- Variabilidad MODERADA (CV={cv:.0f}%)\")\n",
        "else:\n",
        "    print(f\"- Variabilidad ALTA (CV={cv:.0f}%) - Proceso poco predecible\")\n",
        "\n",
        "# Análisis de cola derecha\n",
        "tail_percentage = len(df[df[downtime_col] > p90]) / len(df) * 100\n",
        "if tail_percentage > 15:\n",
        "    print(f\"- Cola derecha extensa ({tail_percentage:.1f}% de datos > P90)\")\n",
        "    print(\"  Hay eventos de paro prolongado que afectan productividad\")\n",
        "\n",
        "# Recomendaciones\n",
        "print(\"\\nRecomendaciones:\")\n",
        "if p95 > 2 * mean_downtime:\n",
        "    print(\"- Investigar causas de paros extremos (>P95)\")\n",
        "if skewness > 1:\n",
        "    print(\"- Implementar mantenimiento preventivo para reducir paros largos\")\n",
        "if cv > 50:\n",
        "    print(\"- Estandarizar procedimientos para reducir variabilidad\")\n",
        "if mean_downtime > 45:\n",
        "    print(\"- Revisar programas de mantenimiento y eficiencia operativa\")\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)"
      ]
    },
