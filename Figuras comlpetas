        "# ============================================================\n",
        "# FIGURAS 12-15: Subplots 2x2\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"GENERANDO FIGURAS 12-15 (Subplots 2x2)\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Crear figura con 2x2 subplots\n",
        "fig, axes = plt.subplots(2, 2, figsize=(14, 10))\n",
        "fig.suptitle('Fig. 12-15: Análisis multivariable', fontsize=14, fontweight='bold', y=0.98)\n",
        "\n",
        "# ============================================================\n",
        "# FIGURA 12: Densidad de Temperature_C por Shift\n",
        "# ============================================================\n",
        "\n",
        "ax1 = axes[0, 0]\n",
        "\n",
        "turnos = ['Morning', 'Evening', 'Night']\n",
        "colores_turno = [colors_shift[t] for t in turnos]\n",
        "estilos_turno = [line_styles_shift[t] for t in turnos]\n",
        "\n",
        "for turno, color, estilo in zip(turnos, colores_turno, estilos_turno):\n",
        "    datos = df[df[shift_col] == turno][temp_col].dropna()\n",
        "    if len(datos) > 0:\n",
        "        # Calcular KDE\n",
        "        kde = stats.gaussian_kde(datos)\n",
        "        x_range = np.linspace(datos.min(), datos.max(), 200)\n",
        "        ax1.plot(x_range, kde(x_range), color=color, linestyle=estilo, linewidth=2, label=turno)\n",
        "\n",
        "ax1.set_xlabel('Temperature (°C)', fontsize=10)\n",
        "ax1.set_ylabel('Densidad', fontsize=10)\n",
        "ax1.grid(True, linestyle='--', linewidth=0.7, alpha=0.6)\n",
        "ax1.set_title('Fig. 12: Densidad de Temperature_C por Shift', fontsize=11, fontweight='bold')\n",
        "ax1.legend(title='Shift', frameon=True)\n",
        "\n",
        "# Configurar bordes\n",
        "for spine in ax1.spines.values():\n",
        "    spine.set_color('#222')\n",
        "    spine.set_linewidth(1.2)\n",
        "\n",
        "# ============================================================\n",
        "# FIGURA 13: Área apilada Units_Produced por Shift (Batch)\n",
        "# ============================================================\n",
        "\n",
        "ax2 = axes[0, 1]\n",
        "\n",
        "# Preparar datos: suma de unidades por batch y turno\n",
        "batch_turno = df.groupby([batch_col, shift_col])[units_col].sum().unstack(fill_value=0)\n",
        "\n",
        "# Reordenar turnos\n",
        "batch_turno = batch_turno[turnos]\n",
        "\n",
        "# Crear área apilada\n",
        "ax2.stackplot(batch_turno.index, batch_turno.T, labels=turnos,\n",
        "              colors=colores_turno, alpha=0.7)\n",
        "\n",
        "ax2.set_xlabel('Batch', fontsize=10)\n",
        "ax2.set_ylabel('Units Produced (suma)', fontsize=10)\n",
        "ax2.set_xticks(batch_turno.index)\n",
        "ax2.yaxis.set_major_formatter(plt.FuncFormatter(lambda y, _: f'{int(y):,}'))\n",
        "ax2.grid(True, linestyle='--', linewidth=0.7, alpha=0.6)\n",
        "ax2.set_title('Fig. 13: Área apilada Units_Produced por Shift', fontsize=11, fontweight='bold')\n",
        "ax2.legend(title='Shift', loc='upper left', frameon=True)\n",
        "\n",
        "# Configurar bordes\n",
        "for spine in ax2.spines.values():\n",
        "    spine.set_color('#222')\n",
        "    spine.set_linewidth(1.2)\n",
        "\n",
        "# ============================================================\n",
        "# FIGURA 14: Composición de Turnos por Línea (%)\n",
        "# ============================================================\n",
        "\n",
        "ax3 = axes[1, 0]\n",
        "\n",
        "# Calcular porcentajes por línea y turno\n",
        "tabla_conteo = pd.crosstab(df[production_line_col], df[shift_col])\n",
        "tabla_porcentaje = tabla_conteo.div(tabla_conteo.sum(axis=1), axis=0) * 100\n",
        "\n",
        "# Crear barras apiladas\n",
        "bottom = np.zeros(len(tabla_porcentaje.index))\n",
        "for turno, color in zip(turnos, colores_turno):\n",
        "    valores = tabla_porcentaje[turno]\n",
        "    bars = ax3.bar(tabla_porcentaje.index, valores, bottom=bottom,\n",
        "                   label=turno, color=color, edgecolor='black', linewidth=0.5)\n",
        "\n",
        "    # Etiquetar segmentos con valor >= 8%\n",
        "    for i, (idx, val) in enumerate(valores.items()):\n",
        "        if val >= 8:\n",
        "            y_pos = bottom[i] + val/2\n",
        "            ax3.text(i, y_pos, f'{val:.0f}%', ha='center', va='center',\n",
        "                    color='white', fontsize=9, fontweight='bold')\n",
        "\n",
        "    bottom += valores\n",
        "\n",
        "ax3.set_xlabel('Production Line', fontsize=10)\n",
        "ax3.set_ylabel('% de registros', fontsize=10)\n",
        "ax3.yaxis.set_major_formatter(plt.FuncFormatter(lambda y, _: f'{y:.0f}%'))\n",
        "ax3.set_ylim(0, 100)\n",
        "ax3.grid(True, linestyle='--', linewidth=0.7, alpha=0.6, axis='y')\n",
        "ax3.set_title('Fig. 14: Composición de Turnos por Línea (%)', fontsize=11, fontweight='bold')\n",
        "ax3.legend(title='Shift', loc='upper right', frameon=True)\n",
        "\n",
        "# Configurar bordes\n",
        "for spine in ax3.spines.values():\n",
        "    spine.set_color('#222')\n",
        "    spine.set_linewidth(1.2)\n",
        "\n",
        "# ============================================================\n",
        "# FIGURA 15: Units_Produced vs Revenue_USD (por línea)\n",
        "# ============================================================\n",
        "\n",
        "ax4 = axes[1, 1]\n",
        "\n",
        "# Graficar puntos por línea\n",
        "for linea in df[production_line_col].unique():\n",
        "    datos_linea = df[df[production_line_col] == linea]\n",
        "    ax4.scatter(datos_linea[units_col], datos_linea[revenue_col],\n",
        "               label=linea, color=colors_lines[linea], marker=markers_lines[linea],\n",
        "               alpha=0.6, edgecolors='black', linewidth=0.5, s=50)\n",
        "\n",
        "# Añadir tendencia global\n",
        "z = np.polyfit(df[units_col], df[revenue_col], 1)\n",
        "p = np.poly1d(z)\n",
        "ax4.plot(sorted(df[units_col]), p(sorted(df[units_col])),\n",
        "         color='red', linestyle='--', linewidth=2, label='Tendencia global')\n",
        "\n",
        "ax4.set_xlabel('Units Produced', fontsize=10)\n",
        "ax4.set_ylabel('Revenue USD', fontsize=10)\n",
        "ax4.yaxis.set_major_formatter(plt.FuncFormatter(lambda y, _: f'${int(y):,}'))\n",
        "ax4.grid(True, linestyle='--', linewidth=0.7, alpha=0.6)\n",
        "ax4.set_title('Fig. 15: Units_Produced vs Revenue_USD (por línea)', fontsize=11, fontweight='bold')\n",
        "ax4.legend(title='Production Line', loc='best', frameon=True)\n",
        "\n",
        "# Configurar bordes\n",
        "for spine in ax4.spines.values():\n",
        "    spine.set_color('#222')\n",
        "    spine.set_linewidth(1.2)\n",
        "\n",
        "# Ajustar layout\n",
        "plt.tight_layout()\n",
        "plt.subplots_adjust(top=0.95)\n",
        "plt.show()\n",
        "\n",
        "# ============================================================\n",
        "# INTERPRETACIÓN DE LAS FIGURAS 12-15\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"INTERPRETACIÓN FIGURAS 12-15\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# ============================================================\n",
        "# FIGURA 12: Densidad de Temperatura por Turno\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"FIGURA 12: Densidad de Temperature_C por Shift\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "for turno in turnos:\n",
        "    datos = df[df[shift_col] == turno][temp_col].dropna()\n",
        "    if len(datos) > 0:\n",
        "        print(f\"\\n{turno}:\")\n",
        "        print(f\"  Media: {datos.mean():.1f}°C\")\n",
        "        print(f\"  Mediana: {datos.median():.1f}°C\")\n",
        "        print(f\"  Std: {datos.std():.1f}°C\")\n",
        "        print(f\"  Rango: {datos.min():.1f}°C - {datos.max():.1f}°C\")\n",
        "\n",
        "# Comparar turnos\n",
        "print(\"\\nCOMPARACIÓN ENTRE TURNOS:\")\n",
        "medias_temp = {turno: df[df[shift_col] == turno][temp_col].mean() for turno in turnos}\n",
        "turno_mas_caliente = max(medias_temp, key=medias_temp.get)\n",
        "turno_mas_frio = min(medias_temp, key=medias_temp.get)\n",
        "\n",
        "print(f\"Turno más caliente: {turno_mas_caliente} ({medias_temp[turno_mas_caliente]:.1f}°C)\")\n",
        "print(f\"Turno más frío: {turno_mas_frio} ({medias_temp[turno_mas_frio]:.1f}°C)\")\n",
        "print(f\"Diferencia: {medias_temp[turno_mas_caliente] - medias_temp[turno_mas_frio]:.1f}°C\")\n",
        "\n",
        "# Prueba estadística\n",
        "from scipy import stats\n",
        "f_stat, p_value = stats.f_oneway(\n",
        "    df[df[shift_col] == 'Morning'][temp_col].dropna(),\n",
        "    df[df[shift_col] == 'Evening'][temp_col].dropna(),\n",
        "    df[df[shift_col] == 'Night'][temp_col].dropna()\n",
        ")\n",
        "print(f\"\\nANOVA p-value: {p_value:.4f}\")\n",
        "if p_value < 0.05:\n",
        "    print(\"→ Diferencias significativas de temperatura entre turnos\")\n",
        "else:\n",
        "    print(\"→ No hay diferencias significativas de temperatura entre turnos\")\n",
        "\n",
        "# ============================================================\n",
        "# FIGURA 13: Área apilada por turno\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"FIGURA 13: Área apilada Units_Produced por Shift\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Contribución total por turno\n",
        "total_por_turno = df.groupby(shift_col)[units_col].sum()\n",
        "print(\"\\nProducción total por turno:\")\n",
        "for turno in turnos:\n",
        "    print(f\"  {turno}: {total_por_turno[turno]:,.0f} unidades ({total_por_turno[turno]/total_por_turno.sum()*100:.1f}%)\")\n",
        "\n",
        "# Evolución por batch\n",
        "print(\"\\nEvolución por batch:\")\n",
        "for batch in sorted(df[batch_col].unique()):\n",
        "    prod_batch = df[df[batch_col] == batch].groupby(shift_col)[units_col].sum()\n",
        "    print(f\"\\nBatch {batch}:\")\n",
        "    for turno in turnos:\n",
        "        if turno in prod_batch.index:\n",
        "            print(f\"  {turno}: {prod_batch[turno]:,.0f} unidades\")\n",
        "\n",
        "# Identificar turno dominante\n",
        "turno_dominante = total_por_turno.idxmax()\n",
        "print(f\"\\nTurno con mayor producción total: {turno_dominante}\")\n",
        "\n",
        "# ============================================================\n",
        "# FIGURA 14: Composición de Turnos por Línea\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"FIGURA 14: Composición de Turnos por Línea (%)\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "print(\"\\nDistribución de turnos por línea (%):\")\n",
        "print(tabla_porcentaje.round(1))\n",
        "\n",
        "# Análisis por línea\n",
        "print(\"\\nANÁLISIS POR LÍNEA:\")\n",
        "for linea in tabla_porcentaje.index:\n",
        "    print(f\"\\n{linea}:\")\n",
        "    turno_principal = tabla_porcentaje.loc[linea].idxmax()\n",
        "    pct_principal = tabla_porcentaje.loc[linea, turno_principal]\n",
        "    print(f\"  Turno principal: {turno_principal} ({pct_principal:.1f}%)\")\n",
        "\n",
        "    # Evaluar balance\n",
        "    if pct_principal > 50:\n",
        "        print(f\"  → Dependencia alta del turno {turno_principal}\")\n",
        "    elif pct_principal > 40:\n",
        "        print(f\"  → Balance moderado\")\n",
        "    else:\n",
        "        print(f\"  → Turnos bien balanceados\")\n",
        "\n",
        "# Comparar composición entre líneas\n",
        "print(\"\\nCOMPARACIÓN ENTRE LÍNEAS:\")\n",
        "for turno in turnos:\n",
        "    valores = tabla_porcentaje[turno]\n",
        "    linea_max_turno = valores.idxmax()\n",
        "    linea_min_turno = valores.idxmin()\n",
        "    print(f\"\\n{turno}:\")\n",
        "    print(f\"  Mayor presencia: {linea_max_turno} ({valores[linea_max_turno]:.1f}%)\")\n",
        "    print(f\"  Menor presencia: {linea_min_turno} ({valores[linea_min_turno]:.1f}%)\")\n",
        "\n",
        "# ============================================================\n",
        "# FIGURA 15: Units vs Revenue por línea\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"FIGURA 15: Units_Produced vs Revenue_USD (por línea)\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Correlación global\n",
        "corr_global = df[units_col].corr(df[revenue_col])\n",
        "print(f\"\\nCorrelación global: {corr_global:.3f}\")\n",
        "\n",
        "# Regresión lineal global\n",
        "slope, intercept, r_value, p_value, std_err = stats.linregress(df[units_col], df[revenue_col])\n",
        "print(f\"Pendiente global: ${slope:.2f} por unidad\")\n",
        "print(f\"R²: {r_value**2:.3f}\")\n",
        "\n",
        "# Por línea\n",
        "print(\"\\nANÁLISIS POR LÍNEA:\")\n",
        "for linea in df[production_line_col].unique():\n",
        "    datos = df[df[production_line_col] == linea]\n",
        "    if len(datos) > 1:\n",
        "        corr = datos[units_col].corr(datos[revenue_col])\n",
        "        slope_l, intercept_l, r_l, p_l, se_l = stats.linregress(datos[units_col], datos[revenue_col])\n",
        "\n",
        "        print(f\"\\n{linea}:\")\n",
        "        print(f\"  Correlación: {corr:.3f}\")\n",
        "        print(f\"  Pendiente: ${slope_l:.2f} por unidad\")\n",
        "        print(f\"  R²: {r_l**2:.3f}\")\n",
        "\n",
        "        # Evaluar eficiencia\n",
        "        if slope_l > slope * 1.1:\n",
        "            print(f\"  → Mayor retorno por unidad que el promedio\")\n",
        "        elif slope_l < slope * 0.9:\n",
        "            print(f\"  → Menor retorno por unidad que el promedio\")\n",
        "        else:\n",
        "            print(f\"  → Retorno por unidad similar al promedio\")\n",
        "\n",
        "# Identificar outliers\n",
        "print(\"\\nOUTLIERS DESTACADOS:\")\n",
        "Q1 = df[revenue_col].quantile(0.25)\n",
        "Q3 = df[revenue_col].quantile(0.75)\n",
        "IQR = Q3 - Q1\n",
        "upper_bound = Q3 + 1.5 * IQR\n",
        "\n",
        "outliers_revenue = df[df[revenue_col] > upper_bound]\n",
        "if len(outliers_revenue) > 0:\n",
        "    for idx, row in outliers_revenue.iterrows():\n",
        "        print(f\"  Lote {int(row[product_id_col])}: ${row[revenue_col]:,.0f} ingresos, {row[units_col]} unidades\")\n",
        "        print(f\"    Línea: {row[production_line_col]}, Turno: {row[shift_col]}\")\n",
        "\n",
        "# ============================================================\n",
        "# RESUMEN INTEGRADO\n",
        "# ============================================================\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)\n",
        "print(\"RESUMEN INTEGRADO DE FIGURAS 12-15\")\n",
        "print(\"=\"*60)\n",
        "\n",
        "# Hallazgos clave\n",
        "print(\"\\nHALLAZGOS CLAVE:\")\n",
        "\n",
        "# 1. Temperatura\n",
        "print(f\"1. Temperatura: El turno {turno_mas_caliente} es el más caliente\")\n",
        "if p_value < 0.05:\n",
        "    print(\"   → Diferencias significativas entre turnos\")\n",
        "\n",
        "# 2. Producción por turno\n",
        "print(f\"2. Producción: {turno_dominante} es el turno más productivo\")\n",
        "\n",
        "# 3. Composición por línea\n",
        "lineas_desbalanceadas = []\n",
        "for linea in tabla_porcentaje.index:\n",
        "    max_turno = tabla_porcentaje.loc[linea].max()\n",
        "    if max_turno > 50:\n",
        "        lineas_desbalanceadas.append((linea, max_turno))\n",
        "\n",
        "if lineas_desbalanceadas:\n",
        "    print(\"3. Composición: Líneas con desbalance de turnos:\")\n",
        "    for linea, pct in lineas_desbalanceadas:\n",
        "        print(f\"   → {linea}: {pct:.1f}% en un solo turno\")\n",
        "\n",
        "# 4. Relación Units-Revenue\n",
        "print(f\"4. Relación Units-Revenue: R² global = {r_value**2:.3f}\")\n",
        "if r_value**2 > 0.7:\n",
        "    print(\"   → Fuerte relación lineal (modelo predictivo viable)\")\n",
        "elif r_value**2 > 0.4:\n",
        "    print(\"   → Relación moderada (otros factores influyen)\")\n",
        "else:\n",
        "    print(\"   → Relación débil (producción no predice ingresos)\")\n",
        "\n",
        "# Recomendaciones integradas\n",
        "print(\"\\nRECOMENDACIONES INTEGRADAS:\")\n",
        "\n",
        "recomendaciones = []\n",
        "\n",
        "# Basado en temperatura\n",
        "if medias_temp[turno_mas_caliente] - medias_temp[turno_mas_frio] > 3:\n",
        "    recomendaciones.append(\"• Investigar causas de diferencia térmica entre turnos\")\n",
        "    recomendaciones.append(\"  (posible impacto en calidad o confort)\")\n",
        "\n",
        "# Basado en producción por turno\n",
        "if total_por_turno[turno_dominante] / total_por_turno.sum() > 0.4:\n",
        "    recomendaciones.append(f\"• Alta dependencia del turno {turno_dominante}\")\n",
        "    recomendaciones.append(\"  Evaluar redistribución de carga\")\n",
        "\n",
        "# Basado en composición\n",
        "for linea, pct in lineas_desbalanceadas:\n",
        "    if pct > 60:\n",
        "        recomendaciones.append(f\"• {linea} muy dependiente de un turno ({pct:.0f}%)\")\n",
        "        recomendaciones.append(\"  Revisar programación para balancear\")\n",
        "\n",
        "# Basado en relación Units-Revenue\n",
        "if slope_l < slope * 0.8:\n",
        "    recomendaciones.append(\"• Líneas con bajo retorno por unidad: revisar precios o costos\")\n",
        "\n",
        "# Mostrar recomendaciones\n",
        "if recomendaciones:\n",
        "    print(\"\\n\" + \"\\n\".join(recomendaciones[:6]))\n",
        "else:\n",
        "    print(\"\\nSin recomendaciones específicas - operación balanceada\")\n",
        "\n",
        "print(\"\\n\" + \"=\"*60)"
